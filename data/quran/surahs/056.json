{
  "id": 56,
  "nameArabic": "الواقعة",
  "nameSimple": "Al-Waqi'ah",
  "nameComplex": "Al-Wāqi`ah",
  "nameTranslation": "The Inevitable",
  "revelationType": "makkah",
  "versesCount": 96,
  "verses": [
    {
      "id": 4980,
      "verseKey": "56:1",
      "verseNumber": 1,
      "textUthmani": " إِذَا وَقَعَتِ ٱلْوَاقِعَةُ",
      "textSimple": "إِذَا وَقَعَتِ الْوَاقِعَةُ"
    },
    {
      "id": 4981,
      "verseKey": "56:2",
      "verseNumber": 2,
      "textUthmani": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ",
      "textSimple": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ"
    },
    {
      "id": 4982,
      "verseKey": "56:3",
      "verseNumber": 3,
      "textUthmani": "خَافِضَةٌ رَّافِعَةٌ",
      "textSimple": "خَافِضَةٌ رَّافِعَةٌ"
    },
    {
      "id": 4983,
      "verseKey": "56:4",
      "verseNumber": 4,
      "textUthmani": "إِذَا رُجَّتِ ٱلْأَرْضُ رَجًّا",
      "textSimple": "إِذَا رُجَّتِ الْأَرْضُ رَجًّا"
    },
    {
      "id": 4984,
      "verseKey": "56:5",
      "verseNumber": 5,
      "textUthmani": "وَبُسَّتِ ٱلْجِبَالُ بَسًّا",
      "textSimple": "وَبُسَّتِ الْجِبَالُ بَسًّا"
    },
    {
      "id": 4985,
      "verseKey": "56:6",
      "verseNumber": 6,
      "textUthmani": "فَكَانَتْ هَبَآءً مُّنۢبَثًّا",
      "textSimple": "فَكَانَتْ هَبَاءً مُّنبَثًّا"
    },
    {
      "id": 4986,
      "verseKey": "56:7",
      "verseNumber": 7,
      "textUthmani": "وَكُنتُمْ أَزْوَٰجًا ثَلَـٰثَةً",
      "textSimple": "وَكُنتُمْ أَزْوَاجًا ثَلَاثَةً"
    },
    {
      "id": 4987,
      "verseKey": "56:8",
      "verseNumber": 8,
      "textUthmani": "فَأَصْحَـٰبُ ٱلْمَيْمَنَةِ مَآ أَصْحَـٰبُ ٱلْمَيْمَنَةِ",
      "textSimple": "فَأَصْحَابُ الْمَيْمَنَةِ مَا أَصْحَابُ الْمَيْمَنَةِ"
    },
    {
      "id": 4988,
      "verseKey": "56:9",
      "verseNumber": 9,
      "textUthmani": "وَأَصْحَـٰبُ ٱلْمَشْـَٔمَةِ مَآ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ",
      "textSimple": "وَأَصْحَابُ الْمَشْأَمَةِ مَا أَصْحَابُ الْمَشْأَمَةِ"
    },
    {
      "id": 4989,
      "verseKey": "56:10",
      "verseNumber": 10,
      "textUthmani": "وَٱلسَّـٰبِقُونَ ٱلسَّـٰبِقُونَ",
      "textSimple": "وَالسَّابِقُونَ السَّابِقُونَ"
    },
    {
      "id": 4990,
      "verseKey": "56:11",
      "verseNumber": 11,
      "textUthmani": "أُو۟لَـٰٓئِكَ ٱلْمُقَرَّبُونَ",
      "textSimple": "أُولَٰئِكَ الْمُقَرَّبُونَ"
    },
    {
      "id": 4991,
      "verseKey": "56:12",
      "verseNumber": 12,
      "textUthmani": "فِى جَنَّـٰتِ ٱلنَّعِيمِ",
      "textSimple": "فِي جَنَّاتِ النَّعِيمِ"
    },
    {
      "id": 4992,
      "verseKey": "56:13",
      "verseNumber": 13,
      "textUthmani": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ",
      "textSimple": "ثُلَّةٌ مِّنَ الْأَوَّلِينَ"
    },
    {
      "id": 4993,
      "verseKey": "56:14",
      "verseNumber": 14,
      "textUthmani": "وَقَلِيلٌ مِّنَ ٱلْـَٔاخِرِينَ",
      "textSimple": "وَقَلِيلٌ مِّنَ الْآخِرِينَ"
    },
    {
      "id": 4994,
      "verseKey": "56:15",
      "verseNumber": 15,
      "textUthmani": "عَلَىٰ سُرُرٍ مَّوْضُونَةٍ",
      "textSimple": "عَلَىٰ سُرُرٍ مَّوْضُونَةٍ"
    },
    {
      "id": 4995,
      "verseKey": "56:16",
      "verseNumber": 16,
      "textUthmani": "مُّتَّكِـِٔينَ عَلَيْهَا مُتَقَـٰبِلِينَ",
      "textSimple": "مُّتَّكِئِينَ عَلَيْهَا مُتَقَابِلِينَ"
    },
    {
      "id": 4996,
      "verseKey": "56:17",
      "verseNumber": 17,
      "textUthmani": "يَطُوفُ عَلَيْهِمْ وِلْدَٰنٌ مُّخَلَّدُونَ",
      "textSimple": "يَطُوفُ عَلَيْهِمْ وِلْدَانٌ مُّخَلَّدُونَ"
    },
    {
      "id": 4997,
      "verseKey": "56:18",
      "verseNumber": 18,
      "textUthmani": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ",
      "textSimple": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ"
    },
    {
      "id": 4998,
      "verseKey": "56:19",
      "verseNumber": 19,
      "textUthmani": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ",
      "textSimple": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ"
    },
    {
      "id": 4999,
      "verseKey": "56:20",
      "verseNumber": 20,
      "textUthmani": "وَفَـٰكِهَةٍ مِّمَّا يَتَخَيَّرُونَ",
      "textSimple": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُونَ"
    },
    {
      "id": 5000,
      "verseKey": "56:21",
      "verseNumber": 21,
      "textUthmani": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ",
      "textSimple": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ"
    },
    {
      "id": 5001,
      "verseKey": "56:22",
      "verseNumber": 22,
      "textUthmani": "وَحُورٌ عِينٌ",
      "textSimple": "وَحُورٌ عِينٌ"
    },
    {
      "id": 5002,
      "verseKey": "56:23",
      "verseNumber": 23,
      "textUthmani": "كَأَمْثَـٰلِ ٱللُّؤْلُؤِ ٱلْمَكْنُونِ",
      "textSimple": "كَأَمْثَالِ اللُّؤْلُؤِ الْمَكْنُونِ"
    },
    {
      "id": 5003,
      "verseKey": "56:24",
      "verseNumber": 24,
      "textUthmani": "جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ",
      "textSimple": "جَزَاءً بِمَا كَانُوا يَعْمَلُونَ"
    },
    {
      "id": 5004,
      "verseKey": "56:25",
      "verseNumber": 25,
      "textUthmani": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا",
      "textSimple": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا"
    },
    {
      "id": 5005,
      "verseKey": "56:26",
      "verseNumber": 26,
      "textUthmani": "إِلَّا قِيلًا سَلَـٰمًا سَلَـٰمًا",
      "textSimple": "إِلَّا قِيلًا سَلَامًا سَلَامًا"
    },
    {
      "id": 5006,
      "verseKey": "56:27",
      "verseNumber": 27,
      "textUthmani": "وَأَصْحَـٰبُ ٱلْيَمِينِ مَآ أَصْحَـٰبُ ٱلْيَمِينِ",
      "textSimple": "وَأَصْحَابُ الْيَمِينِ مَا أَصْحَابُ الْيَمِينِ"
    },
    {
      "id": 5007,
      "verseKey": "56:28",
      "verseNumber": 28,
      "textUthmani": "فِى سِدْرٍ مَّخْضُودٍ",
      "textSimple": "فِي سِدْرٍ مَّخْضُودٍ"
    },
    {
      "id": 5008,
      "verseKey": "56:29",
      "verseNumber": 29,
      "textUthmani": "وَطَلْحٍ مَّنضُودٍ",
      "textSimple": "وَطَلْحٍ مَّنضُودٍ"
    },
    {
      "id": 5009,
      "verseKey": "56:30",
      "verseNumber": 30,
      "textUthmani": "وَظِلٍّ مَّمْدُودٍ",
      "textSimple": "وَظِلٍّ مَّمْدُودٍ"
    },
    {
      "id": 5010,
      "verseKey": "56:31",
      "verseNumber": 31,
      "textUthmani": "وَمَآءٍ مَّسْكُوبٍ",
      "textSimple": "وَمَاءٍ مَّسْكُوبٍ"
    },
    {
      "id": 5011,
      "verseKey": "56:32",
      "verseNumber": 32,
      "textUthmani": "وَفَـٰكِهَةٍ كَثِيرَةٍ",
      "textSimple": "وَفَاكِهَةٍ كَثِيرَةٍ"
    },
    {
      "id": 5012,
      "verseKey": "56:33",
      "verseNumber": 33,
      "textUthmani": "لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ",
      "textSimple": "لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ"
    },
    {
      "id": 5013,
      "verseKey": "56:34",
      "verseNumber": 34,
      "textUthmani": "وَفُرُشٍ مَّرْفُوعَةٍ",
      "textSimple": "وَفُرُشٍ مَّرْفُوعَةٍ"
    },
    {
      "id": 5014,
      "verseKey": "56:35",
      "verseNumber": 35,
      "textUthmani": "إِنَّآ أَنشَأْنَـٰهُنَّ إِنشَآءً",
      "textSimple": "إِنَّا أَنشَأْنَاهُنَّ إِنشَاءً"
    },
    {
      "id": 5015,
      "verseKey": "56:36",
      "verseNumber": 36,
      "textUthmani": "فَجَعَلْنَـٰهُنَّ أَبْكَارًا",
      "textSimple": "فَجَعَلْنَاهُنَّ أَبْكَارًا"
    },
    {
      "id": 5016,
      "verseKey": "56:37",
      "verseNumber": 37,
      "textUthmani": "عُرُبًا أَتْرَابًا",
      "textSimple": "عُرُبًا أَتْرَابًا"
    },
    {
      "id": 5017,
      "verseKey": "56:38",
      "verseNumber": 38,
      "textUthmani": "لِّأَصْحَـٰبِ ٱلْيَمِينِ",
      "textSimple": "لِّأَصْحَابِ الْيَمِينِ"
    },
    {
      "id": 5018,
      "verseKey": "56:39",
      "verseNumber": 39,
      "textUthmani": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ",
      "textSimple": "ثُلَّةٌ مِّنَ الْأَوَّلِينَ"
    },
    {
      "id": 5019,
      "verseKey": "56:40",
      "verseNumber": 40,
      "textUthmani": "وَثُلَّةٌ مِّنَ ٱلْـَٔاخِرِينَ",
      "textSimple": "وَثُلَّةٌ مِّنَ الْآخِرِينَ"
    },
    {
      "id": 5020,
      "verseKey": "56:41",
      "verseNumber": 41,
      "textUthmani": "وَأَصْحَـٰبُ ٱلشِّمَالِ مَآ أَصْحَـٰبُ ٱلشِّمَالِ",
      "textSimple": "وَأَصْحَابُ الشِّمَالِ مَا أَصْحَابُ الشِّمَالِ"
    },
    {
      "id": 5021,
      "verseKey": "56:42",
      "verseNumber": 42,
      "textUthmani": "فِى سَمُومٍ وَحَمِيمٍ",
      "textSimple": "فِي سَمُومٍ وَحَمِيمٍ"
    },
    {
      "id": 5022,
      "verseKey": "56:43",
      "verseNumber": 43,
      "textUthmani": "وَظِلٍّ مِّن يَحْمُومٍ",
      "textSimple": "وَظِلٍّ مِّن يَحْمُومٍ"
    },
    {
      "id": 5023,
      "verseKey": "56:44",
      "verseNumber": 44,
      "textUthmani": "لَّا بَارِدٍ وَلَا كَرِيمٍ",
      "textSimple": "لَّا بَارِدٍ وَلَا كَرِيمٍ"
    },
    {
      "id": 5024,
      "verseKey": "56:45",
      "verseNumber": 45,
      "textUthmani": "إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُتْرَفِينَ",
      "textSimple": "إِنَّهُمْ كَانُوا قَبْلَ ذَٰلِكَ مُتْرَفِينَ"
    },
    {
      "id": 5025,
      "verseKey": "56:46",
      "verseNumber": 46,
      "textUthmani": "وَكَانُوا۟ يُصِرُّونَ عَلَى ٱلْحِنثِ ٱلْعَظِيمِ",
      "textSimple": "وَكَانُوا يُصِرُّونَ عَلَى الْحِنثِ الْعَظِيمِ"
    },
    {
      "id": 5026,
      "verseKey": "56:47",
      "verseNumber": 47,
      "textUthmani": "وَكَانُوا۟ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ",
      "textSimple": "وَكَانُوا يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
    },
    {
      "id": 5027,
      "verseKey": "56:48",
      "verseNumber": 48,
      "textUthmani": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
      "textSimple": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
    },
    {
      "id": 5028,
      "verseKey": "56:49",
      "verseNumber": 49,
      "textUthmani": "قُلْ إِنَّ ٱلْأَوَّلِينَ وَٱلْـَٔاخِرِينَ",
      "textSimple": "قُلْ إِنَّ الْأَوَّلِينَ وَالْآخِرِينَ"
    },
    {
      "id": 5029,
      "verseKey": "56:50",
      "verseNumber": 50,
      "textUthmani": "لَمَجْمُوعُونَ إِلَىٰ مِيقَـٰتِ يَوْمٍ مَّعْلُومٍ",
      "textSimple": "لَمَجْمُوعُونَ إِلَىٰ مِيقَاتِ يَوْمٍ مَّعْلُومٍ"
    },
    {
      "id": 5030,
      "verseKey": "56:51",
      "verseNumber": 51,
      "textUthmani": "ثُمَّ إِنَّكُمْ أَيُّهَا ٱلضَّآلُّونَ ٱلْمُكَذِّبُونَ",
      "textSimple": "ثُمَّ إِنَّكُمْ أَيُّهَا الضَّالُّونَ الْمُكَذِّبُونَ"
    },
    {
      "id": 5031,
      "verseKey": "56:52",
      "verseNumber": 52,
      "textUthmani": "لَـَٔاكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ",
      "textSimple": "لَآكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ"
    },
    {
      "id": 5032,
      "verseKey": "56:53",
      "verseNumber": 53,
      "textUthmani": "فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
      "textSimple": "فَمَالِئُونَ مِنْهَا الْبُطُونَ"
    },
    {
      "id": 5033,
      "verseKey": "56:54",
      "verseNumber": 54,
      "textUthmani": "فَشَـٰرِبُونَ عَلَيْهِ مِنَ ٱلْحَمِيمِ",
      "textSimple": "فَشَارِبُونَ عَلَيْهِ مِنَ الْحَمِيمِ"
    },
    {
      "id": 5034,
      "verseKey": "56:55",
      "verseNumber": 55,
      "textUthmani": "فَشَـٰرِبُونَ شُرْبَ ٱلْهِيمِ",
      "textSimple": "فَشَارِبُونَ شُرْبَ الْهِيمِ"
    },
    {
      "id": 5035,
      "verseKey": "56:56",
      "verseNumber": 56,
      "textUthmani": "هَـٰذَا نُزُلُهُمْ يَوْمَ ٱلدِّينِ",
      "textSimple": "هَٰذَا نُزُلُهُمْ يَوْمَ الدِّينِ"
    },
    {
      "id": 5036,
      "verseKey": "56:57",
      "verseNumber": 57,
      "textUthmani": "نَحْنُ خَلَقْنَـٰكُمْ فَلَوْلَا تُصَدِّقُونَ",
      "textSimple": "نَحْنُ خَلَقْنَاكُمْ فَلَوْلَا تُصَدِّقُونَ"
    },
    {
      "id": 5037,
      "verseKey": "56:58",
      "verseNumber": 58,
      "textUthmani": "أَفَرَءَيْتُم مَّا تُمْنُونَ",
      "textSimple": "أَفَرَأَيْتُم مَّا تُمْنُونَ"
    },
    {
      "id": 5038,
      "verseKey": "56:59",
      "verseNumber": 59,
      "textUthmani": "ءَأَنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ ٱلْخَـٰلِقُونَ",
      "textSimple": "أَأَنتُمْ تَخْلُقُونَهُ أَمْ نَحْنُ الْخَالِقُونَ"
    },
    {
      "id": 5039,
      "verseKey": "56:60",
      "verseNumber": 60,
      "textUthmani": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ ٱلْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ",
      "textSimple": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"
    },
    {
      "id": 5040,
      "verseKey": "56:61",
      "verseNumber": 61,
      "textUthmani": "عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَـٰلَكُمْ وَنُنشِئَكُمْ فِى مَا لَا تَعْلَمُونَ",
      "textSimple": "عَلَىٰ أَن نُّبَدِّلَ أَمْثَالَكُمْ وَنُنشِئَكُمْ فِي مَا لَا تَعْلَمُونَ"
    },
    {
      "id": 5041,
      "verseKey": "56:62",
      "verseNumber": 62,
      "textUthmani": "وَلَقَدْ عَلِمْتُمُ ٱلنَّشْأَةَ ٱلْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ",
      "textSimple": "وَلَقَدْ عَلِمْتُمُ النَّشْأَةَ الْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ"
    },
    {
      "id": 5042,
      "verseKey": "56:63",
      "verseNumber": 63,
      "textUthmani": "أَفَرَءَيْتُم مَّا تَحْرُثُونَ",
      "textSimple": "أَفَرَأَيْتُم مَّا تَحْرُثُونَ"
    },
    {
      "id": 5043,
      "verseKey": "56:64",
      "verseNumber": 64,
      "textUthmani": "ءَأَنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ ٱلزَّٰرِعُونَ",
      "textSimple": "أَأَنتُمْ تَزْرَعُونَهُ أَمْ نَحْنُ الزَّارِعُونَ"
    },
    {
      "id": 5044,
      "verseKey": "56:65",
      "verseNumber": 65,
      "textUthmani": "لَوْ نَشَآءُ لَجَعَلْنَـٰهُ حُطَـٰمًا فَظَلْتُمْ تَفَكَّهُونَ",
      "textSimple": "لَوْ نَشَاءُ لَجَعَلْنَاهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُونَ"
    },
    {
      "id": 5045,
      "verseKey": "56:66",
      "verseNumber": 66,
      "textUthmani": "إِنَّا لَمُغْرَمُونَ",
      "textSimple": "إِنَّا لَمُغْرَمُونَ"
    },
    {
      "id": 5046,
      "verseKey": "56:67",
      "verseNumber": 67,
      "textUthmani": "بَلْ نَحْنُ مَحْرُومُونَ",
      "textSimple": "بَلْ نَحْنُ مَحْرُومُونَ"
    },
    {
      "id": 5047,
      "verseKey": "56:68",
      "verseNumber": 68,
      "textUthmani": "أَفَرَءَيْتُمُ ٱلْمَآءَ ٱلَّذِى تَشْرَبُونَ",
      "textSimple": "أَفَرَأَيْتُمُ الْمَاءَ الَّذِي تَشْرَبُونَ"
    },
    {
      "id": 5048,
      "verseKey": "56:69",
      "verseNumber": 69,
      "textUthmani": "ءَأَنتُمْ أَنزَلْتُمُوهُ مِنَ ٱلْمُزْنِ أَمْ نَحْنُ ٱلْمُنزِلُونَ",
      "textSimple": "أَأَنتُمْ أَنزَلْتُمُوهُ مِنَ الْمُزْنِ أَمْ نَحْنُ الْمُنزِلُونَ"
    },
    {
      "id": 5049,
      "verseKey": "56:70",
      "verseNumber": 70,
      "textUthmani": "لَوْ نَشَآءُ جَعَلْنَـٰهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ",
      "textSimple": "لَوْ نَشَاءُ جَعَلْنَاهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ"
    },
    {
      "id": 5050,
      "verseKey": "56:71",
      "verseNumber": 71,
      "textUthmani": "أَفَرَءَيْتُمُ ٱلنَّارَ ٱلَّتِى تُورُونَ",
      "textSimple": "أَفَرَأَيْتُمُ النَّارَ الَّتِي تُورُونَ"
    },
    {
      "id": 5051,
      "verseKey": "56:72",
      "verseNumber": 72,
      "textUthmani": "ءَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ ٱلْمُنشِـُٔونَ",
      "textSimple": "أَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَا أَمْ نَحْنُ الْمُنشِئُونَ"
    },
    {
      "id": 5052,
      "verseKey": "56:73",
      "verseNumber": 73,
      "textUthmani": "نَحْنُ جَعَلْنَـٰهَا تَذْكِرَةً وَمَتَـٰعًا لِّلْمُقْوِينَ",
      "textSimple": "نَحْنُ جَعَلْنَاهَا تَذْكِرَةً وَمَتَاعًا لِّلْمُقْوِينَ"
    },
    {
      "id": 5053,
      "verseKey": "56:74",
      "verseNumber": 74,
      "textUthmani": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ",
      "textSimple": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
    },
    {
      "id": 5054,
      "verseKey": "56:75",
      "verseNumber": 75,
      "textUthmani": "۞ فَلَآ أُقْسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ",
      "textSimple": "فَلَا أُقْسِمُ بِمَوَاقِعِ النُّجُومِ"
    },
    {
      "id": 5055,
      "verseKey": "56:76",
      "verseNumber": 76,
      "textUthmani": "وَإِنَّهُۥ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ",
      "textSimple": "وَإِنَّهُ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ"
    },
    {
      "id": 5056,
      "verseKey": "56:77",
      "verseNumber": 77,
      "textUthmani": "إِنَّهُۥ لَقُرْءَانٌ كَرِيمٌ",
      "textSimple": "إِنَّهُ لَقُرْآنٌ كَرِيمٌ"
    },
    {
      "id": 5057,
      "verseKey": "56:78",
      "verseNumber": 78,
      "textUthmani": "فِى كِتَـٰبٍ مَّكْنُونٍ",
      "textSimple": "فِي كِتَابٍ مَّكْنُونٍ"
    },
    {
      "id": 5058,
      "verseKey": "56:79",
      "verseNumber": 79,
      "textUthmani": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلْمُطَهَّرُونَ",
      "textSimple": "لَّا يَمَسُّهُ إِلَّا الْمُطَهَّرُونَ"
    },
    {
      "id": 5059,
      "verseKey": "56:80",
      "verseNumber": 80,
      "textUthmani": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ",
      "textSimple": "تَنزِيلٌ مِّن رَّبِّ الْعَالَمِينَ"
    },
    {
      "id": 5060,
      "verseKey": "56:81",
      "verseNumber": 81,
      "textUthmani": "أَفَبِهَـٰذَا ٱلْحَدِيثِ أَنتُم مُّدْهِنُونَ",
      "textSimple": "أَفَبِهَٰذَا الْحَدِيثِ أَنتُم مُّدْهِنُونَ"
    },
    {
      "id": 5061,
      "verseKey": "56:82",
      "verseNumber": 82,
      "textUthmani": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ",
      "textSimple": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ"
    },
    {
      "id": 5062,
      "verseKey": "56:83",
      "verseNumber": 83,
      "textUthmani": "فَلَوْلَآ إِذَا بَلَغَتِ ٱلْحُلْقُومَ",
      "textSimple": "فَلَوْلَا إِذَا بَلَغَتِ الْحُلْقُومَ"
    },
    {
      "id": 5063,
      "verseKey": "56:84",
      "verseNumber": 84,
      "textUthmani": "وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ",
      "textSimple": "وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ"
    },
    {
      "id": 5064,
      "verseKey": "56:85",
      "verseNumber": 85,
      "textUthmani": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَـٰكِن لَّا تُبْصِرُونَ",
      "textSimple": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَٰكِن لَّا تُبْصِرُونَ"
    },
    {
      "id": 5065,
      "verseKey": "56:86",
      "verseNumber": 86,
      "textUthmani": "فَلَوْلَآ إِن كُنتُمْ غَيْرَ مَدِينِينَ",
      "textSimple": "فَلَوْلَا إِن كُنتُمْ غَيْرَ مَدِينِينَ"
    },
    {
      "id": 5066,
      "verseKey": "56:87",
      "verseNumber": 87,
      "textUthmani": "تَرْجِعُونَهَآ إِن كُنتُمْ صَـٰدِقِينَ",
      "textSimple": "تَرْجِعُونَهَا إِن كُنتُمْ صَادِقِينَ"
    },
    {
      "id": 5067,
      "verseKey": "56:88",
      "verseNumber": 88,
      "textUthmani": "فَأَمَّآ إِن كَانَ مِنَ ٱلْمُقَرَّبِينَ",
      "textSimple": "فَأَمَّا إِن كَانَ مِنَ الْمُقَرَّبِينَ"
    },
    {
      "id": 5068,
      "verseKey": "56:89",
      "verseNumber": 89,
      "textUthmani": "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ",
      "textSimple": "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ"
    },
    {
      "id": 5069,
      "verseKey": "56:90",
      "verseNumber": 90,
      "textUthmani": "وَأَمَّآ إِن كَانَ مِنْ أَصْحَـٰبِ ٱلْيَمِينِ",
      "textSimple": "وَأَمَّا إِن كَانَ مِنْ أَصْحَابِ الْيَمِينِ"
    },
    {
      "id": 5070,
      "verseKey": "56:91",
      "verseNumber": 91,
      "textUthmani": "فَسَلَـٰمٌ لَّكَ مِنْ أَصْحَـٰبِ ٱلْيَمِينِ",
      "textSimple": "فَسَلَامٌ لَّكَ مِنْ أَصْحَابِ الْيَمِينِ"
    },
    {
      "id": 5071,
      "verseKey": "56:92",
      "verseNumber": 92,
      "textUthmani": "وَأَمَّآ إِن كَانَ مِنَ ٱلْمُكَذِّبِينَ ٱلضَّآلِّينَ",
      "textSimple": "وَأَمَّا إِن كَانَ مِنَ الْمُكَذِّبِينَ الضَّالِّينَ"
    },
    {
      "id": 5072,
      "verseKey": "56:93",
      "verseNumber": 93,
      "textUthmani": "فَنُزُلٌ مِّنْ حَمِيمٍ",
      "textSimple": "فَنُزُلٌ مِّنْ حَمِيمٍ"
    },
    {
      "id": 5073,
      "verseKey": "56:94",
      "verseNumber": 94,
      "textUthmani": "وَتَصْلِيَةُ جَحِيمٍ",
      "textSimple": "وَتَصْلِيَةُ جَحِيمٍ"
    },
    {
      "id": 5074,
      "verseKey": "56:95",
      "verseNumber": 95,
      "textUthmani": "إِنَّ هَـٰذَا لَهُوَ حَقُّ ٱلْيَقِينِ",
      "textSimple": "إِنَّ هَٰذَا لَهُوَ حَقُّ الْيَقِينِ"
    },
    {
      "id": 5075,
      "verseKey": "56:96",
      "verseNumber": 96,
      "textUthmani": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ",
      "textSimple": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
    }
  ]
}