{
  "id": 86,
  "nameArabic": "الطارق",
  "nameSimple": "At-Tariq",
  "nameComplex": "Aţ-Ţāriq",
  "nameTranslation": "The Nightcommer",
  "revelationType": "makkah",
  "versesCount": 17,
  "verses": [
    {
      "id": 5932,
      "verseKey": "86:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "textSimple": "وَالسَّمَاءِ وَالطَّارِقِ"
    },
    {
      "id": 5933,
      "verseKey": "86:2",
      "verseNumber": 2,
      "textUthmani": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
      "textSimple": "وَمَا أَدْرَاكَ مَا الطَّارِقُ"
    },
    {
      "id": 5934,
      "verseKey": "86:3",
      "verseNumber": 3,
      "textUthmani": "ٱلنَّجْمُ ٱلثَّاقِبُ",
      "textSimple": "النَّجْمُ الثَّاقِبُ"
    },
    {
      "id": 5935,
      "verseKey": "86:4",
      "verseNumber": 4,
      "textUthmani": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ",
      "textSimple": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ"
    },
    {
      "id": 5936,
      "verseKey": "86:5",
      "verseNumber": 5,
      "textUthmani": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ مِمَّ خُلِقَ",
      "textSimple": "فَلْيَنظُرِ الْإِنسَانُ مِمَّ خُلِقَ"
    },
    {
      "id": 5937,
      "verseKey": "86:6",
      "verseNumber": 6,
      "textUthmani": "خُلِقَ مِن مَّآءٍ دَافِقٍ",
      "textSimple": "خُلِقَ مِن مَّاءٍ دَافِقٍ"
    },
    {
      "id": 5938,
      "verseKey": "86:7",
      "verseNumber": 7,
      "textUthmani": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
      "textSimple": "يَخْرُجُ مِن بَيْنِ الصُّلْبِ وَالتَّرَائِبِ"
    },
    {
      "id": 5939,
      "verseKey": "86:8",
      "verseNumber": 8,
      "textUthmani": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌ",
      "textSimple": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ"
    },
    {
      "id": 5940,
      "verseKey": "86:9",
      "verseNumber": 9,
      "textUthmani": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
      "textSimple": "يَوْمَ تُبْلَى السَّرَائِرُ"
    },
    {
      "id": 5941,
      "verseKey": "86:10",
      "verseNumber": 10,
      "textUthmani": "فَمَا لَهُۥ مِن قُوَّةٍ وَلَا نَاصِرٍ",
      "textSimple": "فَمَا لَهُ مِن قُوَّةٍ وَلَا نَاصِرٍ"
    },
    {
      "id": 5942,
      "verseKey": "86:11",
      "verseNumber": 11,
      "textUthmani": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
      "textSimple": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ"
    },
    {
      "id": 5943,
      "verseKey": "86:12",
      "verseNumber": 12,
      "textUthmani": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
      "textSimple": "وَالْأَرْضِ ذَاتِ الصَّدْعِ"
    },
    {
      "id": 5944,
      "verseKey": "86:13",
      "verseNumber": 13,
      "textUthmani": "إِنَّهُۥ لَقَوْلٌ فَصْلٌ",
      "textSimple": "إِنَّهُ لَقَوْلٌ فَصْلٌ"
    },
    {
      "id": 5945,
      "verseKey": "86:14",
      "verseNumber": 14,
      "textUthmani": "وَمَا هُوَ بِٱلْهَزْلِ",
      "textSimple": "وَمَا هُوَ بِالْهَزْلِ"
    },
    {
      "id": 5946,
      "verseKey": "86:15",
      "verseNumber": 15,
      "textUthmani": "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
      "textSimple": "إِنَّهُمْ يَكِيدُونَ كَيْدًا"
    },
    {
      "id": 5947,
      "verseKey": "86:16",
      "verseNumber": 16,
      "textUthmani": "وَأَكِيدُ كَيْدًا",
      "textSimple": "وَأَكِيدُ كَيْدًا"
    },
    {
      "id": 5948,
      "verseKey": "86:17",
      "verseNumber": 17,
      "textUthmani": "فَمَهِّلِ ٱلْكَـٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
      "textSimple": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا"
    }
  ]
}