{
  "id": 80,
  "nameArabic": "عبس",
  "nameSimple": "'Abasa",
  "nameComplex": "`Abasa",
  "nameTranslation": "He Frowned",
  "revelationType": "makkah",
  "versesCount": 42,
  "verses": [
    {
      "id": 5759,
      "verseKey": "80:1",
      "verseNumber": 1,
      "textUthmani": " عَبَسَ وَتَوَلَّىٰٓ",
      "textSimple": "عَبَسَ وَتَوَلَّىٰ"
    },
    {
      "id": 5760,
      "verseKey": "80:2",
      "verseNumber": 2,
      "textUthmani": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
      "textSimple": "أَن جَاءَهُ الْأَعْمَىٰ"
    },
    {
      "id": 5761,
      "verseKey": "80:3",
      "verseNumber": 3,
      "textUthmani": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "textSimple": "وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ"
    },
    {
      "id": 5762,
      "verseKey": "80:4",
      "verseNumber": 4,
      "textUthmani": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
      "textSimple": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ الذِّكْرَىٰ"
    },
    {
      "id": 5763,
      "verseKey": "80:5",
      "verseNumber": 5,
      "textUthmani": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
      "textSimple": "أَمَّا مَنِ اسْتَغْنَىٰ"
    },
    {
      "id": 5764,
      "verseKey": "80:6",
      "verseNumber": 6,
      "textUthmani": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "textSimple": "فَأَنتَ لَهُ تَصَدَّىٰ"
    },
    {
      "id": 5765,
      "verseKey": "80:7",
      "verseNumber": 7,
      "textUthmani": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "textSimple": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ"
    },
    {
      "id": 5766,
      "verseKey": "80:8",
      "verseNumber": 8,
      "textUthmani": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
      "textSimple": "وَأَمَّا مَن جَاءَكَ يَسْعَىٰ"
    },
    {
      "id": 5767,
      "verseKey": "80:9",
      "verseNumber": 9,
      "textUthmani": "وَهُوَ يَخْشَىٰ",
      "textSimple": "وَهُوَ يَخْشَىٰ"
    },
    {
      "id": 5768,
      "verseKey": "80:10",
      "verseNumber": 10,
      "textUthmani": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
      "textSimple": "فَأَنتَ عَنْهُ تَلَهَّىٰ"
    },
    {
      "id": 5769,
      "verseKey": "80:11",
      "verseNumber": 11,
      "textUthmani": "كَلَّآ إِنَّهَا تَذْكِرَةٌ",
      "textSimple": "كَلَّا إِنَّهَا تَذْكِرَةٌ"
    },
    {
      "id": 5770,
      "verseKey": "80:12",
      "verseNumber": 12,
      "textUthmani": "فَمَن شَآءَ ذَكَرَهُۥ",
      "textSimple": "فَمَن شَاءَ ذَكَرَهُ"
    },
    {
      "id": 5771,
      "verseKey": "80:13",
      "verseNumber": 13,
      "textUthmani": "فِى صُحُفٍ مُّكَرَّمَةٍ",
      "textSimple": "فِي صُحُفٍ مُّكَرَّمَةٍ"
    },
    {
      "id": 5772,
      "verseKey": "80:14",
      "verseNumber": 14,
      "textUthmani": "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ",
      "textSimple": "مَّرْفُوعَةٍ مُّطَهَّرَةٍ"
    },
    {
      "id": 5773,
      "verseKey": "80:15",
      "verseNumber": 15,
      "textUthmani": "بِأَيْدِى سَفَرَةٍ",
      "textSimple": "بِأَيْدِي سَفَرَةٍ"
    },
    {
      "id": 5774,
      "verseKey": "80:16",
      "verseNumber": 16,
      "textUthmani": "كِرَامٍۭ بَرَرَةٍ",
      "textSimple": "كِرَامٍ بَرَرَةٍ"
    },
    {
      "id": 5775,
      "verseKey": "80:17",
      "verseNumber": 17,
      "textUthmani": "قُتِلَ ٱلْإِنسَـٰنُ مَآ أَكْفَرَهُۥ",
      "textSimple": "قُتِلَ الْإِنسَانُ مَا أَكْفَرَهُ"
    },
    {
      "id": 5776,
      "verseKey": "80:18",
      "verseNumber": 18,
      "textUthmani": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
      "textSimple": "مِنْ أَيِّ شَيْءٍ خَلَقَهُ"
    },
    {
      "id": 5777,
      "verseKey": "80:19",
      "verseNumber": 19,
      "textUthmani": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "textSimple": "مِن نُّطْفَةٍ خَلَقَهُ فَقَدَّرَهُ"
    },
    {
      "id": 5778,
      "verseKey": "80:20",
      "verseNumber": 20,
      "textUthmani": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "textSimple": "ثُمَّ السَّبِيلَ يَسَّرَهُ"
    },
    {
      "id": 5779,
      "verseKey": "80:21",
      "verseNumber": 21,
      "textUthmani": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
      "textSimple": "ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ"
    },
    {
      "id": 5780,
      "verseKey": "80:22",
      "verseNumber": 22,
      "textUthmani": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "textSimple": "ثُمَّ إِذَا شَاءَ أَنشَرَهُ"
    },
    {
      "id": 5781,
      "verseKey": "80:23",
      "verseNumber": 23,
      "textUthmani": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
      "textSimple": "كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ"
    },
    {
      "id": 5782,
      "verseKey": "80:24",
      "verseNumber": 24,
      "textUthmani": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "textSimple": "فَلْيَنظُرِ الْإِنسَانُ إِلَىٰ طَعَامِهِ"
    },
    {
      "id": 5783,
      "verseKey": "80:25",
      "verseNumber": 25,
      "textUthmani": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا",
      "textSimple": "أَنَّا صَبَبْنَا الْمَاءَ صَبًّا"
    },
    {
      "id": 5784,
      "verseKey": "80:26",
      "verseNumber": 26,
      "textUthmani": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا",
      "textSimple": "ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا"
    },
    {
      "id": 5785,
      "verseKey": "80:27",
      "verseNumber": 27,
      "textUthmani": "فَأَنۢبَتْنَا فِيهَا حَبًّا",
      "textSimple": "فَأَنبَتْنَا فِيهَا حَبًّا"
    },
    {
      "id": 5786,
      "verseKey": "80:28",
      "verseNumber": 28,
      "textUthmani": "وَعِنَبًا وَقَضْبًا",
      "textSimple": "وَعِنَبًا وَقَضْبًا"
    },
    {
      "id": 5787,
      "verseKey": "80:29",
      "verseNumber": 29,
      "textUthmani": "وَزَيْتُونًا وَنَخْلًا",
      "textSimple": "وَزَيْتُونًا وَنَخْلًا"
    },
    {
      "id": 5788,
      "verseKey": "80:30",
      "verseNumber": 30,
      "textUthmani": "وَحَدَآئِقَ غُلْبًا",
      "textSimple": "وَحَدَائِقَ غُلْبًا"
    },
    {
      "id": 5789,
      "verseKey": "80:31",
      "verseNumber": 31,
      "textUthmani": "وَفَـٰكِهَةً وَأَبًّا",
      "textSimple": "وَفَاكِهَةً وَأَبًّا"
    },
    {
      "id": 5790,
      "verseKey": "80:32",
      "verseNumber": 32,
      "textUthmani": "مَّتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "textSimple": "مَّتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ"
    },
    {
      "id": 5791,
      "verseKey": "80:33",
      "verseNumber": 33,
      "textUthmani": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "textSimple": "فَإِذَا جَاءَتِ الصَّاخَّةُ"
    },
    {
      "id": 5792,
      "verseKey": "80:34",
      "verseNumber": 34,
      "textUthmani": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
      "textSimple": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ"
    },
    {
      "id": 5793,
      "verseKey": "80:35",
      "verseNumber": 35,
      "textUthmani": "وَأُمِّهِۦ وَأَبِيهِ",
      "textSimple": "وَأُمِّهِ وَأَبِيهِ"
    },
    {
      "id": 5794,
      "verseKey": "80:36",
      "verseNumber": 36,
      "textUthmani": "وَصَـٰحِبَتِهِۦ وَبَنِيهِ",
      "textSimple": "وَصَاحِبَتِهِ وَبَنِيهِ"
    },
    {
      "id": 5795,
      "verseKey": "80:37",
      "verseNumber": 37,
      "textUthmani": "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "textSimple": "لِكُلِّ امْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ"
    },
    {
      "id": 5796,
      "verseKey": "80:38",
      "verseNumber": 38,
      "textUthmani": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
      "textSimple": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ"
    },
    {
      "id": 5797,
      "verseKey": "80:39",
      "verseNumber": 39,
      "textUthmani": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
      "textSimple": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ"
    },
    {
      "id": 5798,
      "verseKey": "80:40",
      "verseNumber": 40,
      "textUthmani": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "textSimple": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ"
    },
    {
      "id": 5799,
      "verseKey": "80:41",
      "verseNumber": 41,
      "textUthmani": "تَرْهَقُهَا قَتَرَةٌ",
      "textSimple": "تَرْهَقُهَا قَتَرَةٌ"
    },
    {
      "id": 5800,
      "verseKey": "80:42",
      "verseNumber": 42,
      "textUthmani": "أُو۟لَـٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
      "textSimple": "أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ"
    }
  ]
}