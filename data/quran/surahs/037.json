{
  "id": 37,
  "nameArabic": "الصافات",
  "nameSimple": "As-Saffat",
  "nameComplex": "Aş-Şāffāt",
  "nameTranslation": "Those who set the Ranks",
  "revelationType": "makkah",
  "versesCount": 182,
  "verses": [
    {
      "id": 3789,
      "verseKey": "37:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلصَّـٰٓفَّـٰتِ صَفًّا",
      "textSimple": "وَالصَّافَّاتِ صَفًّا"
    },
    {
      "id": 3790,
      "verseKey": "37:2",
      "verseNumber": 2,
      "textUthmani": "فَٱلزَّٰجِرَٰتِ زَجْرًا",
      "textSimple": "فَالزَّاجِرَاتِ زَجْرًا"
    },
    {
      "id": 3791,
      "verseKey": "37:3",
      "verseNumber": 3,
      "textUthmani": "فَٱلتَّـٰلِيَـٰتِ ذِكْرًا",
      "textSimple": "فَالتَّالِيَاتِ ذِكْرًا"
    },
    {
      "id": 3792,
      "verseKey": "37:4",
      "verseNumber": 4,
      "textUthmani": "إِنَّ إِلَـٰهَكُمْ لَوَٰحِدٌ",
      "textSimple": "إِنَّ إِلَٰهَكُمْ لَوَاحِدٌ"
    },
    {
      "id": 3793,
      "verseKey": "37:5",
      "verseNumber": 5,
      "textUthmani": "رَّبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ ٱلْمَشَـٰرِقِ",
      "textSimple": "رَّبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ"
    },
    {
      "id": 3794,
      "verseKey": "37:6",
      "verseNumber": 6,
      "textUthmani": "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِزِينَةٍ ٱلْكَوَاكِبِ",
      "textSimple": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ"
    },
    {
      "id": 3795,
      "verseKey": "37:7",
      "verseNumber": 7,
      "textUthmani": "وَحِفْظًا مِّن كُلِّ شَيْطَـٰنٍ مَّارِدٍ",
      "textSimple": "وَحِفْظًا مِّن كُلِّ شَيْطَانٍ مَّارِدٍ"
    },
    {
      "id": 3796,
      "verseKey": "37:8",
      "verseNumber": 8,
      "textUthmani": "لَّا يَسَّمَّعُونَ إِلَى ٱلْمَلَإِ ٱلْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ",
      "textSimple": "لَّا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ"
    },
    {
      "id": 3797,
      "verseKey": "37:9",
      "verseNumber": 9,
      "textUthmani": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ",
      "textSimple": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ"
    },
    {
      "id": 3798,
      "verseKey": "37:10",
      "verseNumber": 10,
      "textUthmani": "إِلَّا مَنْ خَطِفَ ٱلْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٌ ثَاقِبٌ",
      "textSimple": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ"
    },
    {
      "id": 3799,
      "verseKey": "37:11",
      "verseNumber": 11,
      "textUthmani": "فَٱسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَآ ۚ إِنَّا خَلَقْنَـٰهُم مِّن طِينٍ لَّازِبٍۭ",
      "textSimple": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُم مِّن طِينٍ لَّازِبٍ"
    },
    {
      "id": 3800,
      "verseKey": "37:12",
      "verseNumber": 12,
      "textUthmani": "بَلْ عَجِبْتَ وَيَسْخَرُونَ",
      "textSimple": "بَلْ عَجِبْتَ وَيَسْخَرُونَ"
    },
    {
      "id": 3801,
      "verseKey": "37:13",
      "verseNumber": 13,
      "textUthmani": "وَإِذَا ذُكِّرُوا۟ لَا يَذْكُرُونَ",
      "textSimple": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ"
    },
    {
      "id": 3802,
      "verseKey": "37:14",
      "verseNumber": 14,
      "textUthmani": "وَإِذَا رَأَوْا۟ ءَايَةً يَسْتَسْخِرُونَ",
      "textSimple": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ"
    },
    {
      "id": 3803,
      "verseKey": "37:15",
      "verseNumber": 15,
      "textUthmani": "وَقَالُوٓا۟ إِنْ هَـٰذَآ إِلَّا سِحْرٌ مُّبِينٌ",
      "textSimple": "وَقَالُوا إِنْ هَٰذَا إِلَّا سِحْرٌ مُّبِينٌ"
    },
    {
      "id": 3804,
      "verseKey": "37:16",
      "verseNumber": 16,
      "textUthmani": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ",
      "textSimple": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
    },
    {
      "id": 3805,
      "verseKey": "37:17",
      "verseNumber": 17,
      "textUthmani": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
      "textSimple": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
    },
    {
      "id": 3806,
      "verseKey": "37:18",
      "verseNumber": 18,
      "textUthmani": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَ",
      "textSimple": "قُلْ نَعَمْ وَأَنتُمْ دَاخِرُونَ"
    },
    {
      "id": 3807,
      "verseKey": "37:19",
      "verseNumber": 19,
      "textUthmani": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ",
      "textSimple": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ"
    },
    {
      "id": 3808,
      "verseKey": "37:20",
      "verseNumber": 20,
      "textUthmani": "وَقَالُوا۟ يَـٰوَيْلَنَا هَـٰذَا يَوْمُ ٱلدِّينِ",
      "textSimple": "وَقَالُوا يَا وَيْلَنَا هَٰذَا يَوْمُ الدِّينِ"
    },
    {
      "id": 3809,
      "verseKey": "37:21",
      "verseNumber": 21,
      "textUthmani": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
      "textSimple": "هَٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنتُم بِهِ تُكَذِّبُونَ"
    },
    {
      "id": 3810,
      "verseKey": "37:22",
      "verseNumber": 22,
      "textUthmani": "۞ ٱحْشُرُوا۟ ٱلَّذِينَ ظَلَمُوا۟ وَأَزْوَٰجَهُمْ وَمَا كَانُوا۟ يَعْبُدُونَ",
      "textSimple": "احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ"
    },
    {
      "id": 3811,
      "verseKey": "37:23",
      "verseNumber": 23,
      "textUthmani": "مِن دُونِ ٱللَّهِ فَٱهْدُوهُمْ إِلَىٰ صِرَٰطِ ٱلْجَحِيمِ",
      "textSimple": "مِن دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ"
    },
    {
      "id": 3812,
      "verseKey": "37:24",
      "verseNumber": 24,
      "textUthmani": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْـُٔولُونَ",
      "textSimple": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْئُولُونَ"
    },
    {
      "id": 3813,
      "verseKey": "37:25",
      "verseNumber": 25,
      "textUthmani": "مَا لَكُمْ لَا تَنَاصَرُونَ",
      "textSimple": "مَا لَكُمْ لَا تَنَاصَرُونَ"
    },
    {
      "id": 3814,
      "verseKey": "37:26",
      "verseNumber": 26,
      "textUthmani": "بَلْ هُمُ ٱلْيَوْمَ مُسْتَسْلِمُونَ",
      "textSimple": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ"
    },
    {
      "id": 3815,
      "verseKey": "37:27",
      "verseNumber": 27,
      "textUthmani": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ",
      "textSimple": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
    },
    {
      "id": 3816,
      "verseKey": "37:28",
      "verseNumber": 28,
      "textUthmani": "قَالُوٓا۟ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ ٱلْيَمِينِ",
      "textSimple": "قَالُوا إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ"
    },
    {
      "id": 3817,
      "verseKey": "37:29",
      "verseNumber": 29,
      "textUthmani": "قَالُوا۟ بَل لَّمْ تَكُونُوا۟ مُؤْمِنِينَ",
      "textSimple": "قَالُوا بَل لَّمْ تَكُونُوا مُؤْمِنِينَ"
    },
    {
      "id": 3818,
      "verseKey": "37:30",
      "verseNumber": 30,
      "textUthmani": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَـٰنٍۭ ۖ بَلْ كُنتُمْ قَوْمًا طَـٰغِينَ",
      "textSimple": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَانٍ ۖ بَلْ كُنتُمْ قَوْمًا طَاغِينَ"
    },
    {
      "id": 3819,
      "verseKey": "37:31",
      "verseNumber": 31,
      "textUthmani": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ ۖ إِنَّا لَذَآئِقُونَ",
      "textSimple": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ"
    },
    {
      "id": 3820,
      "verseKey": "37:32",
      "verseNumber": 32,
      "textUthmani": "فَأَغْوَيْنَـٰكُمْ إِنَّا كُنَّا غَـٰوِينَ",
      "textSimple": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ"
    },
    {
      "id": 3821,
      "verseKey": "37:33",
      "verseNumber": 33,
      "textUthmani": "فَإِنَّهُمْ يَوْمَئِذٍ فِى ٱلْعَذَابِ مُشْتَرِكُونَ",
      "textSimple": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ"
    },
    {
      "id": 3822,
      "verseKey": "37:34",
      "verseNumber": 34,
      "textUthmani": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
      "textSimple": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
    },
    {
      "id": 3823,
      "verseKey": "37:35",
      "verseNumber": 35,
      "textUthmani": "إِنَّهُمْ كَانُوٓا۟ إِذَا قِيلَ لَهُمْ لَآ إِلَـٰهَ إِلَّا ٱللَّهُ يَسْتَكْبِرُونَ",
      "textSimple": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ"
    },
    {
      "id": 3824,
      "verseKey": "37:36",
      "verseNumber": 36,
      "textUthmani": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓا۟ ءَالِهَتِنَا لِشَاعِرٍ مَّجْنُونٍۭ",
      "textSimple": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَّجْنُونٍ"
    },
    {
      "id": 3825,
      "verseKey": "37:37",
      "verseNumber": 37,
      "textUthmani": "بَلْ جَآءَ بِٱلْحَقِّ وَصَدَّقَ ٱلْمُرْسَلِينَ",
      "textSimple": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ"
    },
    {
      "id": 3826,
      "verseKey": "37:38",
      "verseNumber": 38,
      "textUthmani": "إِنَّكُمْ لَذَآئِقُوا۟ ٱلْعَذَابِ ٱلْأَلِيمِ",
      "textSimple": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ"
    },
    {
      "id": 3827,
      "verseKey": "37:39",
      "verseNumber": 39,
      "textUthmani": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ",
      "textSimple": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ"
    },
    {
      "id": 3828,
      "verseKey": "37:40",
      "verseNumber": 40,
      "textUthmani": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
      "textSimple": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
    },
    {
      "id": 3829,
      "verseKey": "37:41",
      "verseNumber": 41,
      "textUthmani": "أُو۟لَـٰٓئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ",
      "textSimple": "أُولَٰئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ"
    },
    {
      "id": 3830,
      "verseKey": "37:42",
      "verseNumber": 42,
      "textUthmani": "فَوَٰكِهُ ۖ وَهُم مُّكْرَمُونَ",
      "textSimple": "فَوَاكِهُ ۖ وَهُم مُّكْرَمُونَ"
    },
    {
      "id": 3831,
      "verseKey": "37:43",
      "verseNumber": 43,
      "textUthmani": "فِى جَنَّـٰتِ ٱلنَّعِيمِ",
      "textSimple": "فِي جَنَّاتِ النَّعِيمِ"
    },
    {
      "id": 3832,
      "verseKey": "37:44",
      "verseNumber": 44,
      "textUthmani": "عَلَىٰ سُرُرٍ مُّتَقَـٰبِلِينَ",
      "textSimple": "عَلَىٰ سُرُرٍ مُّتَقَابِلِينَ"
    },
    {
      "id": 3833,
      "verseKey": "37:45",
      "verseNumber": 45,
      "textUthmani": "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍۭ",
      "textSimple": "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍ"
    },
    {
      "id": 3834,
      "verseKey": "37:46",
      "verseNumber": 46,
      "textUthmani": "بَيْضَآءَ لَذَّةٍ لِّلشَّـٰرِبِينَ",
      "textSimple": "بَيْضَاءَ لَذَّةٍ لِّلشَّارِبِينَ"
    },
    {
      "id": 3835,
      "verseKey": "37:47",
      "verseNumber": 47,
      "textUthmani": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ",
      "textSimple": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ"
    },
    {
      "id": 3836,
      "verseKey": "37:48",
      "verseNumber": 48,
      "textUthmani": "وَعِندَهُمْ قَـٰصِرَٰتُ ٱلطَّرْفِ عِينٌ",
      "textSimple": "وَعِندَهُمْ قَاصِرَاتُ الطَّرْفِ عِينٌ"
    },
    {
      "id": 3837,
      "verseKey": "37:49",
      "verseNumber": 49,
      "textUthmani": "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ",
      "textSimple": "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ"
    },
    {
      "id": 3838,
      "verseKey": "37:50",
      "verseNumber": 50,
      "textUthmani": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ",
      "textSimple": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
    },
    {
      "id": 3839,
      "verseKey": "37:51",
      "verseNumber": 51,
      "textUthmani": "قَالَ قَآئِلٌ مِّنْهُمْ إِنِّى كَانَ لِى قَرِينٌ",
      "textSimple": "قَالَ قَائِلٌ مِّنْهُمْ إِنِّي كَانَ لِي قَرِينٌ"
    },
    {
      "id": 3840,
      "verseKey": "37:52",
      "verseNumber": 52,
      "textUthmani": "يَقُولُ أَءِنَّكَ لَمِنَ ٱلْمُصَدِّقِينَ",
      "textSimple": "يَقُولُ أَإِنَّكَ لَمِنَ الْمُصَدِّقِينَ"
    },
    {
      "id": 3841,
      "verseKey": "37:53",
      "verseNumber": 53,
      "textUthmani": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَدِينُونَ",
      "textSimple": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَدِينُونَ"
    },
    {
      "id": 3842,
      "verseKey": "37:54",
      "verseNumber": 54,
      "textUthmani": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ",
      "textSimple": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ"
    },
    {
      "id": 3843,
      "verseKey": "37:55",
      "verseNumber": 55,
      "textUthmani": "فَٱطَّلَعَ فَرَءَاهُ فِى سَوَآءِ ٱلْجَحِيمِ",
      "textSimple": "فَاطَّلَعَ فَرَآهُ فِي سَوَاءِ الْجَحِيمِ"
    },
    {
      "id": 3844,
      "verseKey": "37:56",
      "verseNumber": 56,
      "textUthmani": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرْدِينِ",
      "textSimple": "قَالَ تَاللَّهِ إِن كِدتَّ لَتُرْدِينِ"
    },
    {
      "id": 3845,
      "verseKey": "37:57",
      "verseNumber": 57,
      "textUthmani": "وَلَوْلَا نِعْمَةُ رَبِّى لَكُنتُ مِنَ ٱلْمُحْضَرِينَ",
      "textSimple": "وَلَوْلَا نِعْمَةُ رَبِّي لَكُنتُ مِنَ الْمُحْضَرِينَ"
    },
    {
      "id": 3846,
      "verseKey": "37:58",
      "verseNumber": 58,
      "textUthmani": "أَفَمَا نَحْنُ بِمَيِّتِينَ",
      "textSimple": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
    },
    {
      "id": 3847,
      "verseKey": "37:59",
      "verseNumber": 59,
      "textUthmani": "إِلَّا مَوْتَتَنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ",
      "textSimple": "إِلَّا مَوْتَتَنَا الْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ"
    },
    {
      "id": 3848,
      "verseKey": "37:60",
      "verseNumber": 60,
      "textUthmani": "إِنَّ هَـٰذَا لَهُوَ ٱلْفَوْزُ ٱلْعَظِيمُ",
      "textSimple": "إِنَّ هَٰذَا لَهُوَ الْفَوْزُ الْعَظِيمُ"
    },
    {
      "id": 3849,
      "verseKey": "37:61",
      "verseNumber": 61,
      "textUthmani": "لِمِثْلِ هَـٰذَا فَلْيَعْمَلِ ٱلْعَـٰمِلُونَ",
      "textSimple": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ الْعَامِلُونَ"
    },
    {
      "id": 3850,
      "verseKey": "37:62",
      "verseNumber": 62,
      "textUthmani": "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ ٱلزَّقُّومِ",
      "textSimple": "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ الزَّقُّومِ"
    },
    {
      "id": 3851,
      "verseKey": "37:63",
      "verseNumber": 63,
      "textUthmani": "إِنَّا جَعَلْنَـٰهَا فِتْنَةً لِّلظَّـٰلِمِينَ",
      "textSimple": "إِنَّا جَعَلْنَاهَا فِتْنَةً لِّلظَّالِمِينَ"
    },
    {
      "id": 3852,
      "verseKey": "37:64",
      "verseNumber": 64,
      "textUthmani": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِىٓ أَصْلِ ٱلْجَحِيمِ",
      "textSimple": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِي أَصْلِ الْجَحِيمِ"
    },
    {
      "id": 3853,
      "verseKey": "37:65",
      "verseNumber": 65,
      "textUthmani": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّيَـٰطِينِ",
      "textSimple": "طَلْعُهَا كَأَنَّهُ رُءُوسُ الشَّيَاطِينِ"
    },
    {
      "id": 3854,
      "verseKey": "37:66",
      "verseNumber": 66,
      "textUthmani": "فَإِنَّهُمْ لَـَٔاكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
      "textSimple": "فَإِنَّهُمْ لَآكِلُونَ مِنْهَا فَمَالِئُونَ مِنْهَا الْبُطُونَ"
    },
    {
      "id": 3855,
      "verseKey": "37:67",
      "verseNumber": 67,
      "textUthmani": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِّنْ حَمِيمٍ",
      "textSimple": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِّنْ حَمِيمٍ"
    },
    {
      "id": 3856,
      "verseKey": "37:68",
      "verseNumber": 68,
      "textUthmani": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى ٱلْجَحِيمِ",
      "textSimple": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى الْجَحِيمِ"
    },
    {
      "id": 3857,
      "verseKey": "37:69",
      "verseNumber": 69,
      "textUthmani": "إِنَّهُمْ أَلْفَوْا۟ ءَابَآءَهُمْ ضَآلِّينَ",
      "textSimple": "إِنَّهُمْ أَلْفَوْا آبَاءَهُمْ ضَالِّينَ"
    },
    {
      "id": 3858,
      "verseKey": "37:70",
      "verseNumber": 70,
      "textUthmani": "فَهُمْ عَلَىٰٓ ءَاثَـٰرِهِمْ يُهْرَعُونَ",
      "textSimple": "فَهُمْ عَلَىٰ آثَارِهِمْ يُهْرَعُونَ"
    },
    {
      "id": 3859,
      "verseKey": "37:71",
      "verseNumber": 71,
      "textUthmani": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ ٱلْأَوَّلِينَ",
      "textSimple": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ الْأَوَّلِينَ"
    },
    {
      "id": 3860,
      "verseKey": "37:72",
      "verseNumber": 72,
      "textUthmani": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ",
      "textSimple": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ"
    },
    {
      "id": 3861,
      "verseKey": "37:73",
      "verseNumber": 73,
      "textUthmani": "فَٱنظُرْ كَيْفَ كَانَ عَـٰقِبَةُ ٱلْمُنذَرِينَ",
      "textSimple": "فَانظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُنذَرِينَ"
    },
    {
      "id": 3862,
      "verseKey": "37:74",
      "verseNumber": 74,
      "textUthmani": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
      "textSimple": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
    },
    {
      "id": 3863,
      "verseKey": "37:75",
      "verseNumber": 75,
      "textUthmani": "وَلَقَدْ نَادَىٰنَا نُوحٌ فَلَنِعْمَ ٱلْمُجِيبُونَ",
      "textSimple": "وَلَقَدْ نَادَانَا نُوحٌ فَلَنِعْمَ الْمُجِيبُونَ"
    },
    {
      "id": 3864,
      "verseKey": "37:76",
      "verseNumber": 76,
      "textUthmani": "وَنَجَّيْنَـٰهُ وَأَهْلَهُۥ مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
      "textSimple": "وَنَجَّيْنَاهُ وَأَهْلَهُ مِنَ الْكَرْبِ الْعَظِيمِ"
    },
    {
      "id": 3865,
      "verseKey": "37:77",
      "verseNumber": 77,
      "textUthmani": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ ٱلْبَاقِينَ",
      "textSimple": "وَجَعَلْنَا ذُرِّيَّتَهُ هُمُ الْبَاقِينَ"
    },
    {
      "id": 3866,
      "verseKey": "37:78",
      "verseNumber": 78,
      "textUthmani": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
      "textSimple": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
    },
    {
      "id": 3867,
      "verseKey": "37:79",
      "verseNumber": 79,
      "textUthmani": "سَلَـٰمٌ عَلَىٰ نُوحٍ فِى ٱلْعَـٰلَمِينَ",
      "textSimple": "سَلَامٌ عَلَىٰ نُوحٍ فِي الْعَالَمِينَ"
    },
    {
      "id": 3868,
      "verseKey": "37:80",
      "verseNumber": 80,
      "textUthmani": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
      "textSimple": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
    },
    {
      "id": 3869,
      "verseKey": "37:81",
      "verseNumber": 81,
      "textUthmani": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
      "textSimple": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
    },
    {
      "id": 3870,
      "verseKey": "37:82",
      "verseNumber": 82,
      "textUthmani": "ثُمَّ أَغْرَقْنَا ٱلْـَٔاخَرِينَ",
      "textSimple": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
    },
    {
      "id": 3871,
      "verseKey": "37:83",
      "verseNumber": 83,
      "textUthmani": "۞ وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ",
      "textSimple": "وَإِنَّ مِن شِيعَتِهِ لَإِبْرَاهِيمَ"
    },
    {
      "id": 3872,
      "verseKey": "37:84",
      "verseNumber": 84,
      "textUthmani": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبٍ سَلِيمٍ",
      "textSimple": "إِذْ جَاءَ رَبَّهُ بِقَلْبٍ سَلِيمٍ"
    },
    {
      "id": 3873,
      "verseKey": "37:85",
      "verseNumber": 85,
      "textUthmani": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَ",
      "textSimple": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَاذَا تَعْبُدُونَ"
    },
    {
      "id": 3874,
      "verseKey": "37:86",
      "verseNumber": 86,
      "textUthmani": "أَئِفْكًا ءَالِهَةً دُونَ ٱللَّهِ تُرِيدُونَ",
      "textSimple": "أَئِفْكًا آلِهَةً دُونَ اللَّهِ تُرِيدُونَ"
    },
    {
      "id": 3875,
      "verseKey": "37:87",
      "verseNumber": 87,
      "textUthmani": "فَمَا ظَنُّكُم بِرَبِّ ٱلْعَـٰلَمِينَ",
      "textSimple": "فَمَا ظَنُّكُم بِرَبِّ الْعَالَمِينَ"
    },
    {
      "id": 3876,
      "verseKey": "37:88",
      "verseNumber": 88,
      "textUthmani": "فَنَظَرَ نَظْرَةً فِى ٱلنُّجُومِ",
      "textSimple": "فَنَظَرَ نَظْرَةً فِي النُّجُومِ"
    },
    {
      "id": 3877,
      "verseKey": "37:89",
      "verseNumber": 89,
      "textUthmani": "فَقَالَ إِنِّى سَقِيمٌ",
      "textSimple": "فَقَالَ إِنِّي سَقِيمٌ"
    },
    {
      "id": 3878,
      "verseKey": "37:90",
      "verseNumber": 90,
      "textUthmani": "فَتَوَلَّوْا۟ عَنْهُ مُدْبِرِينَ",
      "textSimple": "فَتَوَلَّوْا عَنْهُ مُدْبِرِينَ"
    },
    {
      "id": 3879,
      "verseKey": "37:91",
      "verseNumber": 91,
      "textUthmani": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ",
      "textSimple": "فَرَاغَ إِلَىٰ آلِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ"
    },
    {
      "id": 3880,
      "verseKey": "37:92",
      "verseNumber": 92,
      "textUthmani": "مَا لَكُمْ لَا تَنطِقُونَ",
      "textSimple": "مَا لَكُمْ لَا تَنطِقُونَ"
    },
    {
      "id": 3881,
      "verseKey": "37:93",
      "verseNumber": 93,
      "textUthmani": "فَرَاغَ عَلَيْهِمْ ضَرْبًۢا بِٱلْيَمِينِ",
      "textSimple": "فَرَاغَ عَلَيْهِمْ ضَرْبًا بِالْيَمِينِ"
    },
    {
      "id": 3882,
      "verseKey": "37:94",
      "verseNumber": 94,
      "textUthmani": "فَأَقْبَلُوٓا۟ إِلَيْهِ يَزِفُّونَ",
      "textSimple": "فَأَقْبَلُوا إِلَيْهِ يَزِفُّونَ"
    },
    {
      "id": 3883,
      "verseKey": "37:95",
      "verseNumber": 95,
      "textUthmani": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ",
      "textSimple": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ"
    },
    {
      "id": 3884,
      "verseKey": "37:96",
      "verseNumber": 96,
      "textUthmani": "وَٱللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ",
      "textSimple": "وَاللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ"
    },
    {
      "id": 3885,
      "verseKey": "37:97",
      "verseNumber": 97,
      "textUthmani": "قَالُوا۟ ٱبْنُوا۟ لَهُۥ بُنْيَـٰنًا فَأَلْقُوهُ فِى ٱلْجَحِيمِ",
      "textSimple": "قَالُوا ابْنُوا لَهُ بُنْيَانًا فَأَلْقُوهُ فِي الْجَحِيمِ"
    },
    {
      "id": 3886,
      "verseKey": "37:98",
      "verseNumber": 98,
      "textUthmani": "فَأَرَادُوا۟ بِهِۦ كَيْدًا فَجَعَلْنَـٰهُمُ ٱلْأَسْفَلِينَ",
      "textSimple": "فَأَرَادُوا بِهِ كَيْدًا فَجَعَلْنَاهُمُ الْأَسْفَلِينَ"
    },
    {
      "id": 3887,
      "verseKey": "37:99",
      "verseNumber": 99,
      "textUthmani": "وَقَالَ إِنِّى ذَاهِبٌ إِلَىٰ رَبِّى سَيَهْدِينِ",
      "textSimple": "وَقَالَ إِنِّي ذَاهِبٌ إِلَىٰ رَبِّي سَيَهْدِينِ"
    },
    {
      "id": 3888,
      "verseKey": "37:100",
      "verseNumber": 100,
      "textUthmani": "رَبِّ هَبْ لِى مِنَ ٱلصَّـٰلِحِينَ",
      "textSimple": "رَبِّ هَبْ لِي مِنَ الصَّالِحِينَ"
    },
    {
      "id": 3889,
      "verseKey": "37:101",
      "verseNumber": 101,
      "textUthmani": "فَبَشَّرْنَـٰهُ بِغُلَـٰمٍ حَلِيمٍ",
      "textSimple": "فَبَشَّرْنَاهُ بِغُلَامٍ حَلِيمٍ"
    },
    {
      "id": 3890,
      "verseKey": "37:102",
      "verseNumber": 102,
      "textUthmani": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعْىَ قَالَ يَـٰبُنَىَّ إِنِّىٓ أَرَىٰ فِى ٱلْمَنَامِ أَنِّىٓ أَذْبَحُكَ فَٱنظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَـٰٓأَبَتِ ٱفْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِىٓ إِن شَآءَ ٱللَّهُ مِنَ ٱلصَّـٰبِرِينَ",
      "textSimple": "فَلَمَّا بَلَغَ مَعَهُ السَّعْيَ قَالَ يَا بُنَيَّ إِنِّي أَرَىٰ فِي الْمَنَامِ أَنِّي أَذْبَحُكَ فَانظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَا أَبَتِ افْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِي إِن شَاءَ اللَّهُ مِنَ الصَّابِرِينَ"
