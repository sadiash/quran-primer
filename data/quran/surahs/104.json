    },
    {
      "id": 6181,
      "verseKey": "104:2",
      "verseNumber": 2,
      "textUthmani": "ٱلَّذِى جَمَعَ مَالًا وَعَدَّدَهُۥ",
      "textSimple": "الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ"
    },
    {
      "id": 6182,
      "verseKey": "104:3",
      "verseNumber": 3,
      "textUthmani": "يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ",
      "textSimple": "يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ"
    },
    {
      "id": 6183,
      "verseKey": "104:4",
      "verseNumber": 4,
      "textUthmani": "كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ",
