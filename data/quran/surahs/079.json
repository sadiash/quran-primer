{
  "id": 79,
  "nameArabic": "النازعات",
  "nameSimple": "An-Nazi'at",
  "nameComplex": "An-Nāzi`āt",
  "nameTranslation": "Those who drag forth",
  "revelationType": "makkah",
  "versesCount": 46,
  "verses": [
    {
      "id": 5713,
      "verseKey": "79:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلنَّـٰزِعَـٰتِ غَرْقًا",
      "textSimple": "وَالنَّازِعَاتِ غَرْقًا"
    },
    {
      "id": 5714,
      "verseKey": "79:2",
      "verseNumber": 2,
      "textUthmani": "وَٱلنَّـٰشِطَـٰتِ نَشْطًا",
      "textSimple": "وَالنَّاشِطَاتِ نَشْطًا"
    },
    {
      "id": 5715,
      "verseKey": "79:3",
      "verseNumber": 3,
      "textUthmani": "وَٱلسَّـٰبِحَـٰتِ سَبْحًا",
      "textSimple": "وَالسَّابِحَاتِ سَبْحًا"
    },
    {
      "id": 5716,
      "verseKey": "79:4",
      "verseNumber": 4,
      "textUthmani": "فَٱلسَّـٰبِقَـٰتِ سَبْقًا",
      "textSimple": "فَالسَّابِقَاتِ سَبْقًا"
    },
    {
      "id": 5717,
      "verseKey": "79:5",
      "verseNumber": 5,
      "textUthmani": "فَٱلْمُدَبِّرَٰتِ أَمْرًا",
      "textSimple": "فَالْمُدَبِّرَاتِ أَمْرًا"
    },
    {
      "id": 5718,
      "verseKey": "79:6",
      "verseNumber": 6,
      "textUthmani": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
      "textSimple": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ"
    },
    {
      "id": 5719,
      "verseKey": "79:7",
      "verseNumber": 7,
      "textUthmani": "تَتْبَعُهَا ٱلرَّادِفَةُ",
      "textSimple": "تَتْبَعُهَا الرَّادِفَةُ"
    },
    {
      "id": 5720,
      "verseKey": "79:8",
      "verseNumber": 8,
      "textUthmani": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
      "textSimple": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"
    },
    {
      "id": 5721,
      "verseKey": "79:9",
      "verseNumber": 9,
      "textUthmani": "أَبْصَـٰرُهَا خَـٰشِعَةٌ",
      "textSimple": "أَبْصَارُهَا خَاشِعَةٌ"
    },
    {
      "id": 5722,
      "verseKey": "79:10",
      "verseNumber": 10,
      "textUthmani": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
      "textSimple": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ"
    },
    {
      "id": 5723,
      "verseKey": "79:11",
      "verseNumber": 11,
      "textUthmani": "أَءِذَا كُنَّا عِظَـٰمًا نَّخِرَةً",
      "textSimple": "أَإِذَا كُنَّا عِظَامًا نَّخِرَةً"
    },
    {
      "id": 5724,
      "verseKey": "79:12",
      "verseNumber": 12,
      "textUthmani": "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
      "textSimple": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"
    },
    {
      "id": 5725,
      "verseKey": "79:13",
      "verseNumber": 13,
      "textUthmani": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ",
      "textSimple": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ"
    },
    {
      "id": 5726,
      "verseKey": "79:14",
      "verseNumber": 14,
      "textUthmani": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "textSimple": "فَإِذَا هُم بِالسَّاهِرَةِ"
    },
    {
      "id": 5727,
      "verseKey": "79:15",
      "verseNumber": 15,
      "textUthmani": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "textSimple": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ"
    },
    {
      "id": 5728,
      "verseKey": "79:16",
      "verseNumber": 16,
      "textUthmani": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
      "textSimple": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى"
    },
    {
      "id": 5729,
      "verseKey": "79:17",
      "verseNumber": 17,
      "textUthmani": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
      "textSimple": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ"
    },
    {
      "id": 5730,
      "verseKey": "79:18",
      "verseNumber": 18,
      "textUthmani": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "textSimple": "فَقُلْ هَل لَّكَ إِلَىٰ أَن تَزَكَّىٰ"
    },
    {
      "id": 5731,
      "verseKey": "79:19",
      "verseNumber": 19,
      "textUthmani": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
      "textSimple": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"
    },
    {
      "id": 5732,
      "verseKey": "79:20",
      "verseNumber": 20,
      "textUthmani": "فَأَرَىٰهُ ٱلْـَٔايَةَ ٱلْكُبْرَىٰ",
      "textSimple": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ"
    },
    {
      "id": 5733,
      "verseKey": "79:21",
      "verseNumber": 21,
      "textUthmani": "فَكَذَّبَ وَعَصَىٰ",
      "textSimple": "فَكَذَّبَ وَعَصَىٰ"
    },
    {
      "id": 5734,
      "verseKey": "79:22",
      "verseNumber": 22,
      "textUthmani": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
      "textSimple": "ثُمَّ أَدْبَرَ يَسْعَىٰ"
    },
    {
      "id": 5735,
      "verseKey": "79:23",
      "verseNumber": 23,
      "textUthmani": "فَحَشَرَ فَنَادَىٰ",
      "textSimple": "فَحَشَرَ فَنَادَىٰ"
    },
    {
      "id": 5736,
      "verseKey": "79:24",
      "verseNumber": 24,
      "textUthmani": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
      "textSimple": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ"
    },
    {
      "id": 5737,
      "verseKey": "79:25",
      "verseNumber": 25,
      "textUthmani": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْـَٔاخِرَةِ وَٱلْأُولَىٰٓ",
      "textSimple": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ"
    },
    {
      "id": 5738,
      "verseKey": "79:26",
      "verseNumber": 26,
      "textUthmani": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ",
      "textSimple": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰ"
    },
    {
      "id": 5739,
      "verseKey": "79:27",
      "verseNumber": 27,
      "textUthmani": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
      "textSimple": "أَأَنتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا"
    },
    {
      "id": 5740,
      "verseKey": "79:28",
      "verseNumber": 28,
      "textUthmani": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
      "textSimple": "رَفَعَ سَمْكَهَا فَسَوَّاهَا"
    },
    {
      "id": 5741,
      "verseKey": "79:29",
      "verseNumber": 29,
      "textUthmani": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
      "textSimple": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا"
    },
    {
      "id": 5742,
      "verseKey": "79:30",
      "verseNumber": 30,
      "textUthmani": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
      "textSimple": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا"
    },
    {
      "id": 5743,
      "verseKey": "79:31",
      "verseNumber": 31,
      "textUthmani": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
      "textSimple": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا"
    },
    {
      "id": 5744,
      "verseKey": "79:32",
      "verseNumber": 32,
      "textUthmani": "وَٱلْجِبَالَ أَرْسَىٰهَا",
      "textSimple": "وَالْجِبَالَ أَرْسَاهَا"
    },
    {
      "id": 5745,
      "verseKey": "79:33",
      "verseNumber": 33,
      "textUthmani": "مَتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "textSimple": "مَتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ"
    },
    {
      "id": 5746,
      "verseKey": "79:34",
      "verseNumber": 34,
      "textUthmani": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
      "textSimple": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ"
    },
    {
      "id": 5747,
      "verseKey": "79:35",
      "verseNumber": 35,
      "textUthmani": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَـٰنُ مَا سَعَىٰ",
      "textSimple": "يَوْمَ يَتَذَكَّرُ الْإِنسَانُ مَا سَعَىٰ"
    },
    {
      "id": 5748,
      "verseKey": "79:36",
      "verseNumber": 36,
      "textUthmani": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
      "textSimple": "وَبُرِّزَتِ الْجَحِيمُ لِمَن يَرَىٰ"
    },
    {
      "id": 5749,
      "verseKey": "79:37",
      "verseNumber": 37,
      "textUthmani": "فَأَمَّا مَن طَغَىٰ",
      "textSimple": "فَأَمَّا مَن طَغَىٰ"
    },
    {
      "id": 5750,
      "verseKey": "79:38",
      "verseNumber": 38,
      "textUthmani": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "textSimple": "وَآثَرَ الْحَيَاةَ الدُّنْيَا"
    },
    {
      "id": 5751,
      "verseKey": "79:39",
      "verseNumber": 39,
      "textUthmani": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
      "textSimple": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ"
    },
    {
      "id": 5752,
      "verseKey": "79:40",
      "verseNumber": 40,
      "textUthmani": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
      "textSimple": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ"
    },
    {
      "id": 5753,
      "verseKey": "79:41",
      "verseNumber": 41,
      "textUthmani": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
      "textSimple": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ"
    },
    {
      "id": 5754,
      "verseKey": "79:42",
      "verseNumber": 42,
      "textUthmani": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
      "textSimple": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا"
    },
    {
      "id": 5755,
      "verseKey": "79:43",
      "verseNumber": 43,
      "textUthmani": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
      "textSimple": "فِيمَ أَنتَ مِن ذِكْرَاهَا"
    },
    {
      "id": 5756,
      "verseKey": "79:44",
      "verseNumber": 44,
      "textUthmani": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "textSimple": "إِلَىٰ رَبِّكَ مُنتَهَاهَا"
    },
    {
      "id": 5757,
      "verseKey": "79:45",
      "verseNumber": 45,
      "textUthmani": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
      "textSimple": "إِنَّمَا أَنتَ مُنذِرُ مَن يَخْشَاهَا"
    },
    {
      "id": 5758,
      "verseKey": "79:46",
      "verseNumber": 46,
      "textUthmani": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
      "textSimple": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"
    }
  ]
}