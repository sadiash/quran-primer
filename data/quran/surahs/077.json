{
  "id": 77,
  "nameArabic": "المرسلات",
  "nameSimple": "Al-Mursalat",
  "nameComplex": "Al-Mursalāt",
  "nameTranslation": "The Emissaries",
  "revelationType": "makkah",
  "versesCount": 50,
  "verses": [
    {
      "id": 5623,
      "verseKey": "77:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلْمُرْسَلَـٰتِ عُرْفًا",
      "textSimple": "وَالْمُرْسَلَاتِ عُرْفًا"
    },
    {
      "id": 5624,
      "verseKey": "77:2",
      "verseNumber": 2,
      "textUthmani": "فَٱلْعَـٰصِفَـٰتِ عَصْفًا",
      "textSimple": "فَالْعَاصِفَاتِ عَصْفًا"
    },
    {
      "id": 5625,
      "verseKey": "77:3",
      "verseNumber": 3,
      "textUthmani": "وَٱلنَّـٰشِرَٰتِ نَشْرًا",
      "textSimple": "وَالنَّاشِرَاتِ نَشْرًا"
    },
    {
      "id": 5626,
      "verseKey": "77:4",
      "verseNumber": 4,
      "textUthmani": "فَٱلْفَـٰرِقَـٰتِ فَرْقًا",
      "textSimple": "فَالْفَارِقَاتِ فَرْقًا"
    },
    {
      "id": 5627,
      "verseKey": "77:5",
      "verseNumber": 5,
      "textUthmani": "فَٱلْمُلْقِيَـٰتِ ذِكْرًا",
      "textSimple": "فَالْمُلْقِيَاتِ ذِكْرًا"
    },
    {
      "id": 5628,
      "verseKey": "77:6",
      "verseNumber": 6,
      "textUthmani": "عُذْرًا أَوْ نُذْرًا",
      "textSimple": "عُذْرًا أَوْ نُذْرًا"
    },
    {
      "id": 5629,
      "verseKey": "77:7",
      "verseNumber": 7,
      "textUthmani": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ",
      "textSimple": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ"
    },
    {
      "id": 5630,
      "verseKey": "77:8",
      "verseNumber": 8,
      "textUthmani": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
      "textSimple": "فَإِذَا النُّجُومُ طُمِسَتْ"
    },
    {
      "id": 5631,
      "verseKey": "77:9",
      "verseNumber": 9,
      "textUthmani": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
      "textSimple": "وَإِذَا السَّمَاءُ فُرِجَتْ"
    },
    {
      "id": 5632,
      "verseKey": "77:10",
      "verseNumber": 10,
      "textUthmani": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
      "textSimple": "وَإِذَا الْجِبَالُ نُسِفَتْ"
    },
    {
      "id": 5633,
      "verseKey": "77:11",
      "verseNumber": 11,
      "textUthmani": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
      "textSimple": "وَإِذَا الرُّسُلُ أُقِّتَتْ"
    },
    {
      "id": 5634,
      "verseKey": "77:12",
      "verseNumber": 12,
      "textUthmani": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
      "textSimple": "لِأَيِّ يَوْمٍ أُجِّلَتْ"
    },
    {
      "id": 5635,
      "verseKey": "77:13",
      "verseNumber": 13,
      "textUthmani": "لِيَوْمِ ٱلْفَصْلِ",
      "textSimple": "لِيَوْمِ الْفَصْلِ"
    },
    {
      "id": 5636,
      "verseKey": "77:14",
      "verseNumber": 14,
      "textUthmani": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
      "textSimple": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ"
    },
    {
      "id": 5637,
      "verseKey": "77:15",
      "verseNumber": 15,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5638,
      "verseKey": "77:16",
      "verseNumber": 16,
      "textUthmani": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
      "textSimple": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ"
    },
    {
      "id": 5639,
      "verseKey": "77:17",
      "verseNumber": 17,
      "textUthmani": "ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ",
      "textSimple": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ"
    },
    {
      "id": 5640,
      "verseKey": "77:18",
      "verseNumber": 18,
      "textUthmani": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
      "textSimple": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
    },
    {
      "id": 5641,
      "verseKey": "77:19",
      "verseNumber": 19,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5642,
      "verseKey": "77:20",
      "verseNumber": 20,
      "textUthmani": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ",
      "textSimple": "أَلَمْ نَخْلُقكُّم مِّن مَّاءٍ مَّهِينٍ"
    },
    {
      "id": 5643,
      "verseKey": "77:21",
      "verseNumber": 21,
      "textUthmani": "فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ",
      "textSimple": "فَجَعَلْنَاهُ فِي قَرَارٍ مَّكِينٍ"
    },
    {
      "id": 5644,
      "verseKey": "77:22",
      "verseNumber": 22,
      "textUthmani": "إِلَىٰ قَدَرٍ مَّعْلُومٍ",
      "textSimple": "إِلَىٰ قَدَرٍ مَّعْلُومٍ"
    },
    {
      "id": 5645,
      "verseKey": "77:23",
      "verseNumber": 23,
      "textUthmani": "فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ",
      "textSimple": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ"
    },
    {
      "id": 5646,
      "verseKey": "77:24",
      "verseNumber": 24,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5647,
      "verseKey": "77:25",
      "verseNumber": 25,
      "textUthmani": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
      "textSimple": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا"
    },
    {
      "id": 5648,
      "verseKey": "77:26",
      "verseNumber": 26,
      "textUthmani": "أَحْيَآءً وَأَمْوَٰتًا",
      "textSimple": "أَحْيَاءً وَأَمْوَاتًا"
    },
    {
      "id": 5649,
      "verseKey": "77:27",
      "verseNumber": 27,
      "textUthmani": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا",
      "textSimple": "وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُم مَّاءً فُرَاتًا"
    },
    {
      "id": 5650,
      "verseKey": "77:28",
      "verseNumber": 28,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5651,
      "verseKey": "77:29",
      "verseNumber": 29,
      "textUthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "textSimple": "انطَلِقُوا إِلَىٰ مَا كُنتُم بِهِ تُكَذِّبُونَ"
    },
    {
      "id": 5652,
      "verseKey": "77:30",
      "verseNumber": 30,
      "textUthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ",
      "textSimple": "انطَلِقُوا إِلَىٰ ظِلٍّ ذِي ثَلَاثِ شُعَبٍ"
    },
    {
      "id": 5653,
      "verseKey": "77:31",
      "verseNumber": 31,
      "textUthmani": "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
      "textSimple": "لَّا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ"
    },
    {
      "id": 5654,
      "verseKey": "77:32",
      "verseNumber": 32,
      "textUthmani": "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ",
      "textSimple": "إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ"
    },
    {
      "id": 5655,
      "verseKey": "77:33",
      "verseNumber": 33,
      "textUthmani": "كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ",
      "textSimple": "كَأَنَّهُ جِمَالَتٌ صُفْرٌ"
    },
    {
      "id": 5656,
      "verseKey": "77:34",
      "verseNumber": 34,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5657,
      "verseKey": "77:35",
      "verseNumber": 35,
      "textUthmani": "هَـٰذَا يَوْمُ لَا يَنطِقُونَ",
      "textSimple": "هَٰذَا يَوْمُ لَا يَنطِقُونَ"
    },
    {
      "id": 5658,
      "verseKey": "77:36",
      "verseNumber": 36,
      "textUthmani": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
      "textSimple": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"
    },
    {
      "id": 5659,
      "verseKey": "77:37",
      "verseNumber": 37,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5660,
      "verseKey": "77:38",
      "verseNumber": 38,
      "textUthmani": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ",
      "textSimple": "هَٰذَا يَوْمُ الْفَصْلِ ۖ جَمَعْنَاكُمْ وَالْأَوَّلِينَ"
    },
    {
      "id": 5661,
      "verseKey": "77:39",
      "verseNumber": 39,
      "textUthmani": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
      "textSimple": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"
    },
    {
      "id": 5662,
      "verseKey": "77:40",
      "verseNumber": 40,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5663,
      "verseKey": "77:41",
      "verseNumber": 41,
      "textUthmani": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ",
      "textSimple": "إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ"
    },
    {
      "id": 5664,
      "verseKey": "77:42",
      "verseNumber": 42,
      "textUthmani": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
      "textSimple": "وَفَوَاكِهَ مِمَّا يَشْتَهُونَ"
    },
    {
      "id": 5665,
      "verseKey": "77:43",
      "verseNumber": 43,
      "textUthmani": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
      "textSimple": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنتُمْ تَعْمَلُونَ"
    },
    {
      "id": 5666,
      "verseKey": "77:44",
      "verseNumber": 44,
      "textUthmani": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
      "textSimple": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
    },
    {
      "id": 5667,
      "verseKey": "77:45",
      "verseNumber": 45,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5668,
      "verseKey": "77:46",
      "verseNumber": 46,
      "textUthmani": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
      "textSimple": "كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُم مُّجْرِمُونَ"
    },
    {
      "id": 5669,
      "verseKey": "77:47",
      "verseNumber": 47,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5670,
      "verseKey": "77:48",
      "verseNumber": 48,
      "textUthmani": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
      "textSimple": "وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ"
    },
    {
      "id": 5671,
      "verseKey": "77:49",
      "verseNumber": 49,
      "textUthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "textSimple": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ"
    },
    {
      "id": 5672,
      "verseKey": "77:50",
      "verseNumber": 50,
      "textUthmani": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
      "textSimple": "فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ"
    }
  ]
}