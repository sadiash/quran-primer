{
  "id": 82,
  "nameArabic": "الإنفطار",
  "nameSimple": "Al-Infitar",
  "nameComplex": "Al-'Infiţār",
  "nameTranslation": "The Cleaving",
  "revelationType": "makkah",
  "versesCount": 19,
  "verses": [
    {
      "id": 5830,
      "verseKey": "82:1",
      "verseNumber": 1,
      "textUthmani": " إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ",
      "textSimple": "إِذَا السَّمَاءُ انفَطَرَتْ"
    },
    {
      "id": 5831,
      "verseKey": "82:2",
      "verseNumber": 2,
      "textUthmani": "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ",
      "textSimple": "وَإِذَا الْكَوَاكِبُ انتَثَرَتْ"
    },
    {
      "id": 5832,
      "verseKey": "82:3",
      "verseNumber": 3,
      "textUthmani": "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ",
      "textSimple": "وَإِذَا الْبِحَارُ فُجِّرَتْ"
    },
    {
      "id": 5833,
      "verseKey": "82:4",
      "verseNumber": 4,
      "textUthmani": "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ",
      "textSimple": "وَإِذَا الْقُبُورُ بُعْثِرَتْ"
    },
    {
      "id": 5834,
      "verseKey": "82:5",
      "verseNumber": 5,
      "textUthmani": "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَأَخَّرَتْ",
      "textSimple": "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَأَخَّرَتْ"
    },
    {
      "id": 5835,
      "verseKey": "82:6",
      "verseNumber": 6,
      "textUthmani": "يَـٰٓأَيُّهَا ٱلْإِنسَـٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ",
      "textSimple": "يَا أَيُّهَا الْإِنسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِيمِ"
    },
    {
      "id": 5836,
      "verseKey": "82:7",
      "verseNumber": 7,
      "textUthmani": "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "textSimple": "الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَكَ"
    },
    {
      "id": 5837,
      "verseKey": "82:8",
      "verseNumber": 8,
      "textUthmani": "فِىٓ أَىِّ صُورَةٍ مَّا شَآءَ رَكَّبَكَ",
      "textSimple": "فِي أَيِّ صُورَةٍ مَّا شَاءَ رَكَّبَكَ"
    },
    {
      "id": 5838,
      "verseKey": "82:9",
      "verseNumber": 9,
      "textUthmani": "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ",
      "textSimple": "كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ"
    },
    {
      "id": 5839,
      "verseKey": "82:10",
      "verseNumber": 10,
      "textUthmani": "وَإِنَّ عَلَيْكُمْ لَحَـٰفِظِينَ",
      "textSimple": "وَإِنَّ عَلَيْكُمْ لَحَافِظِينَ"
    },
    {
      "id": 5840,
      "verseKey": "82:11",
      "verseNumber": 11,
      "textUthmani": "كِرَامًا كَـٰتِبِينَ",
      "textSimple": "كِرَامًا كَاتِبِينَ"
    },
    {
      "id": 5841,
      "verseKey": "82:12",
      "verseNumber": 12,
      "textUthmani": "يَعْلَمُونَ مَا تَفْعَلُونَ",
      "textSimple": "يَعْلَمُونَ مَا تَفْعَلُونَ"
    },
    {
      "id": 5842,
      "verseKey": "82:13",
      "verseNumber": 13,
      "textUthmani": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ",
      "textSimple": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
    },
    {
      "id": 5843,
      "verseKey": "82:14",
      "verseNumber": 14,
      "textUthmani": "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍ",
      "textSimple": "وَإِنَّ الْفُجَّارَ لَفِي جَحِيمٍ"
    },
    {
      "id": 5844,
      "verseKey": "82:15",
      "verseNumber": 15,
      "textUthmani": "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ",
      "textSimple": "يَصْلَوْنَهَا يَوْمَ الدِّينِ"
    },
    {
      "id": 5845,
      "verseKey": "82:16",
      "verseNumber": 16,
      "textUthmani": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ",
      "textSimple": "وَمَا هُمْ عَنْهَا بِغَائِبِينَ"
    },
    {
      "id": 5846,
      "verseKey": "82:17",
      "verseNumber": 17,
      "textUthmani": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
      "textSimple": "وَمَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
    },
    {
      "id": 5847,
      "verseKey": "82:18",
      "verseNumber": 18,
      "textUthmani": "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
      "textSimple": "ثُمَّ مَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
    },
    {
      "id": 5848,
      "verseKey": "82:19",
      "verseNumber": 19,
      "textUthmani": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَيْـًٔا ۖ وَٱلْأَمْرُ يَوْمَئِذٍ لِّلَّهِ",
      "textSimple": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَيْئًا ۖ وَالْأَمْرُ يَوْمَئِذٍ لِّلَّهِ"
    }
  ]
}