    {
      "id": 6035,
      "verseKey": "90:12",
      "verseNumber": 12,
      "textUthmani": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
      "textSimple": "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ"
    },
    {
      "id": 6036,
      "verseKey": "90:13",
      "verseNumber": 13,
      "textUthmani": "فَكُّ رَقَبَةٍ",
