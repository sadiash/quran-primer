    },
    {
      "id": 6031,
      "verseKey": "90:8",
      "verseNumber": 8,
      "textUthmani": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
      "textSimple": "أَلَمْ نَجْعَل لَّهُ عَيْنَيْنِ"
    },
    {
      "id": 6032,
      "verseKey": "90:9",
      "verseNumber": 9,
      "textUthmani": "وَلِسَانًا وَشَفَتَيْنِ",
      "textSimple": "وَلِسَانًا وَشَفَتَيْنِ"
    },
    {
      "id": 6033,
      "verseKey": "90:10",
      "verseNumber": 10,
      "textUthmani": "وَهَدَيْنَـٰهُ ٱلنَّجْدَيْنِ",
