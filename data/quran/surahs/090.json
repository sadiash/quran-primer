  "verses": [
    {
      "id": 6024,
      "verseKey": "90:1",
      "verseNumber": 1,
      "textUthmani": " لَآ أُقْسِمُ بِهَـٰذَا ٱلْبَلَدِ",
      "textSimple": "لَا أُقْسِمُ بِهَٰذَا الْبَلَدِ"
    },
    {
      "id": 6025,
      "verseKey": "90:2",
      "verseNumber": 2,
      "textUthmani": "وَأَنتَ حِلٌّۢ بِهَـٰذَا ٱلْبَلَدِ",
      "textSimple": "وَأَنتَ حِلٌّ بِهَٰذَا الْبَلَدِ"
    },
    {
      "id": 6026,
      "verseKey": "90:3",
      "verseNumber": 3,
      "textUthmani": "وَوَالِدٍ وَمَا وَلَدَ",
      "textSimple": "وَوَالِدٍ وَمَا وَلَدَ"
    },
    {
      "id": 6027,
      "verseKey": "90:4",
      "verseNumber": 4,
      "textUthmani": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِى كَبَدٍ",
