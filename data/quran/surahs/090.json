    },
    {
      "id": 6028,
      "verseKey": "90:5",
      "verseNumber": 5,
      "textUthmani": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ",
      "textSimple": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ"
    },
    {
      "id": 6029,
      "verseKey": "90:6",
      "verseNumber": 6,
      "textUthmani": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا",
      "textSimple": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا"
    },
    {
      "id": 6030,
      "verseKey": "90:7",
      "verseNumber": 7,
      "textUthmani": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
