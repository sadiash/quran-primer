{
  "id": 87,
  "nameArabic": "الأعلى",
  "nameSimple": "Al-A'la",
  "nameComplex": "Al-'A`lá",
  "nameTranslation": "The Most High",
  "revelationType": "makkah",
  "versesCount": 19,
  "verses": [
    {
      "id": 5949,
      "verseKey": "87:1",
      "verseNumber": 1,
      "textUthmani": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
      "textSimple": "سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى"
    },
    {
      "id": 5950,
      "verseKey": "87:2",
      "verseNumber": 2,
      "textUthmani": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
      "textSimple": "الَّذِي خَلَقَ فَسَوَّىٰ"
    },
    {
      "id": 5951,
      "verseKey": "87:3",
      "verseNumber": 3,
      "textUthmani": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
      "textSimple": "وَالَّذِي قَدَّرَ فَهَدَىٰ"
    },
    {
      "id": 5952,
      "verseKey": "87:4",
      "verseNumber": 4,
      "textUthmani": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
      "textSimple": "وَالَّذِي أَخْرَجَ الْمَرْعَىٰ"
    },
    {
      "id": 5953,
      "verseKey": "87:5",
      "verseNumber": 5,
      "textUthmani": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
      "textSimple": "فَجَعَلَهُ غُثَاءً أَحْوَىٰ"
    },
    {
      "id": 5954,
      "verseKey": "87:6",
      "verseNumber": 6,
      "textUthmani": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
      "textSimple": "سَنُقْرِئُكَ فَلَا تَنسَىٰ"
    },
    {
      "id": 5955,
      "verseKey": "87:7",
      "verseNumber": 7,
      "textUthmani": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
      "textSimple": "إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ"
    },
    {
      "id": 5956,
      "verseKey": "87:8",
      "verseNumber": 8,
      "textUthmani": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
      "textSimple": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ"
    },
    {
      "id": 5957,
      "verseKey": "87:9",
      "verseNumber": 9,
      "textUthmani": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
      "textSimple": "فَذَكِّرْ إِن نَّفَعَتِ الذِّكْرَىٰ"
    },
    {
      "id": 5958,
      "verseKey": "87:10",
      "verseNumber": 10,
      "textUthmani": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
      "textSimple": "سَيَذَّكَّرُ مَن يَخْشَىٰ"
    },
    {
      "id": 5959,
      "verseKey": "87:11",
      "verseNumber": 11,
      "textUthmani": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
      "textSimple": "وَيَتَجَنَّبُهَا الْأَشْقَى"
    },
    {
      "id": 5960,
      "verseKey": "87:12",
      "verseNumber": 12,
      "textUthmani": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
      "textSimple": "الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ"
    },
    {
      "id": 5961,
      "verseKey": "87:13",
      "verseNumber": 13,
      "textUthmani": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "textSimple": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ"
    },
    {
      "id": 5962,
      "verseKey": "87:14",
      "verseNumber": 14,
      "textUthmani": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
      "textSimple": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ"
    },
    {
      "id": 5963,
      "verseKey": "87:15",
      "verseNumber": 15,
      "textUthmani": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
      "textSimple": "وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ"
    },
    {
      "id": 5964,
      "verseKey": "87:16",
      "verseNumber": 16,
      "textUthmani": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "textSimple": "بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا"
    },
    {
      "id": 5965,
      "verseKey": "87:17",
      "verseNumber": 17,
      "textUthmani": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
      "textSimple": "وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ"
    },
    {
      "id": 5966,
      "verseKey": "87:18",
      "verseNumber": 18,
      "textUthmani": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
      "textSimple": "إِنَّ هَٰذَا لَفِي الصُّحُفِ الْأُولَىٰ"
    },
    {
      "id": 5967,
      "verseKey": "87:19",
      "verseNumber": 19,
      "textUthmani": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
      "textSimple": "صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ"
    }
  ]
}