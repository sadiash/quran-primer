{
  "id": 81,
  "nameArabic": "التكوير",
  "nameSimple": "At-Takwir",
  "nameComplex": "At-Takwīr",
  "nameTranslation": "The Overthrowing",
  "revelationType": "makkah",
  "versesCount": 29,
  "verses": [
    {
      "id": 5801,
      "verseKey": "81:1",
      "verseNumber": 1,
      "textUthmani": " إِذَا ٱلشَّمْسُ كُوِّرَتْ",
      "textSimple": "إِذَا الشَّمْسُ كُوِّرَتْ"
    },
    {
      "id": 5802,
      "verseKey": "81:2",
      "verseNumber": 2,
      "textUthmani": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
      "textSimple": "وَإِذَا النُّجُومُ انكَدَرَتْ"
    },
    {
      "id": 5803,
      "verseKey": "81:3",
      "verseNumber": 3,
      "textUthmani": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
      "textSimple": "وَإِذَا الْجِبَالُ سُيِّرَتْ"
    },
    {
      "id": 5804,
      "verseKey": "81:4",
      "verseNumber": 4,
      "textUthmani": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
      "textSimple": "وَإِذَا الْعِشَارُ عُطِّلَتْ"
    },
    {
      "id": 5805,
      "verseKey": "81:5",
      "verseNumber": 5,
      "textUthmani": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
      "textSimple": "وَإِذَا الْوُحُوشُ حُشِرَتْ"
    },
    {
      "id": 5806,
      "verseKey": "81:6",
      "verseNumber": 6,
      "textUthmani": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
      "textSimple": "وَإِذَا الْبِحَارُ سُجِّرَتْ"
    },
    {
      "id": 5807,
      "verseKey": "81:7",
      "verseNumber": 7,
      "textUthmani": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
      "textSimple": "وَإِذَا النُّفُوسُ زُوِّجَتْ"
    },
    {
      "id": 5808,
      "verseKey": "81:8",
      "verseNumber": 8,
      "textUthmani": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
      "textSimple": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ"
    },
    {
      "id": 5809,
      "verseKey": "81:9",
      "verseNumber": 9,
      "textUthmani": "بِأَىِّ ذَنۢبٍ قُتِلَتْ",
      "textSimple": "بِأَيِّ ذَنبٍ قُتِلَتْ"
    },
    {
      "id": 5810,
      "verseKey": "81:10",
      "verseNumber": 10,
      "textUthmani": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
      "textSimple": "وَإِذَا الصُّحُفُ نُشِرَتْ"
    },
    {
      "id": 5811,
      "verseKey": "81:11",
      "verseNumber": 11,
      "textUthmani": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
      "textSimple": "وَإِذَا السَّمَاءُ كُشِطَتْ"
    },
    {
      "id": 5812,
      "verseKey": "81:12",
      "verseNumber": 12,
      "textUthmani": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
      "textSimple": "وَإِذَا الْجَحِيمُ سُعِّرَتْ"
    },
    {
      "id": 5813,
      "verseKey": "81:13",
      "verseNumber": 13,
      "textUthmani": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
      "textSimple": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ"
    },
    {
      "id": 5814,
      "verseKey": "81:14",
      "verseNumber": 14,
      "textUthmani": "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ",
      "textSimple": "عَلِمَتْ نَفْسٌ مَّا أَحْضَرَتْ"
    },
    {
      "id": 5815,
      "verseKey": "81:15",
      "verseNumber": 15,
      "textUthmani": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
      "textSimple": "فَلَا أُقْسِمُ بِالْخُنَّسِ"
    },
    {
      "id": 5816,
      "verseKey": "81:16",
      "verseNumber": 16,
      "textUthmani": "ٱلْجَوَارِ ٱلْكُنَّسِ",
      "textSimple": "الْجَوَارِ الْكُنَّسِ"
    },
    {
      "id": 5817,
      "verseKey": "81:17",
      "verseNumber": 17,
      "textUthmani": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
      "textSimple": "وَاللَّيْلِ إِذَا عَسْعَسَ"
    },
    {
      "id": 5818,
      "verseKey": "81:18",
      "verseNumber": 18,
      "textUthmani": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
      "textSimple": "وَالصُّبْحِ إِذَا تَنَفَّسَ"
    },
    {
      "id": 5819,
      "verseKey": "81:19",
      "verseNumber": 19,
      "textUthmani": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "textSimple": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
    },
    {
      "id": 5820,
      "verseKey": "81:20",
      "verseNumber": 20,
      "textUthmani": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ",
      "textSimple": "ذِي قُوَّةٍ عِندَ ذِي الْعَرْشِ مَكِينٍ"
    },
    {
      "id": 5821,
      "verseKey": "81:21",
      "verseNumber": 21,
      "textUthmani": "مُّطَاعٍ ثَمَّ أَمِينٍ",
      "textSimple": "مُّطَاعٍ ثَمَّ أَمِينٍ"
    },
    {
      "id": 5822,
      "verseKey": "81:22",
      "verseNumber": 22,
      "textUthmani": "وَمَا صَاحِبُكُم بِمَجْنُونٍ",
      "textSimple": "وَمَا صَاحِبُكُم بِمَجْنُونٍ"
    },
    {
      "id": 5823,
      "verseKey": "81:23",
      "verseNumber": 23,
      "textUthmani": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
      "textSimple": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ"
    },
    {
      "id": 5824,
      "verseKey": "81:24",
      "verseNumber": 24,
      "textUthmani": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ",
      "textSimple": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ"
    },
    {
      "id": 5825,
      "verseKey": "81:25",
      "verseNumber": 25,
      "textUthmani": "وَمَا هُوَ بِقَوْلِ شَيْطَـٰنٍ رَّجِيمٍ",
      "textSimple": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَّجِيمٍ"
    },
    {
      "id": 5826,
      "verseKey": "81:26",
      "verseNumber": 26,
      "textUthmani": "فَأَيْنَ تَذْهَبُونَ",
      "textSimple": "فَأَيْنَ تَذْهَبُونَ"
    },
    {
      "id": 5827,
      "verseKey": "81:27",
      "verseNumber": 27,
      "textUthmani": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ",
      "textSimple": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَالَمِينَ"
    },
    {
      "id": 5828,
      "verseKey": "81:28",
      "verseNumber": 28,
      "textUthmani": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
      "textSimple": "لِمَن شَاءَ مِنكُمْ أَن يَسْتَقِيمَ"
    },
    {
      "id": 5829,
      "verseKey": "81:29",
      "verseNumber": 29,
      "textUthmani": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَـٰلَمِينَ",
      "textSimple": "وَمَا تَشَاءُونَ إِلَّا أَن يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ"
    }
  ]
}