{
  "id": 84,
  "nameArabic": "الإنشقاق",
  "nameSimple": "Al-Inshiqaq",
  "nameComplex": "Al-'Inshiqāq",
  "nameTranslation": "The Sundering",
  "revelationType": "makkah",
  "versesCount": 25,
  "verses": [
    {
      "id": 5885,
      "verseKey": "84:1",
      "verseNumber": 1,
      "textUthmani": " إِذَا ٱلسَّمَآءُ ٱنشَقَّتْ",
      "textSimple": "إِذَا السَّمَاءُ انشَقَّتْ"
    },
    {
      "id": 5886,
      "verseKey": "84:2",
      "verseNumber": 2,
      "textUthmani": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
      "textSimple": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
    },
    {
      "id": 5887,
      "verseKey": "84:3",
      "verseNumber": 3,
      "textUthmani": "وَإِذَا ٱلْأَرْضُ مُدَّتْ",
      "textSimple": "وَإِذَا الْأَرْضُ مُدَّتْ"
    },
    {
      "id": 5888,
      "verseKey": "84:4",
      "verseNumber": 4,
      "textUthmani": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ",
      "textSimple": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ"
    },
    {
      "id": 5889,
      "verseKey": "84:5",
      "verseNumber": 5,
      "textUthmani": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
      "textSimple": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
    },
    {
      "id": 5890,
      "verseKey": "84:6",
      "verseNumber": 6,
      "textUthmani": "يَـٰٓأَيُّهَا ٱلْإِنسَـٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَـٰقِيهِ",
      "textSimple": "يَا أَيُّهَا الْإِنسَانُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَاقِيهِ"
    },
    {
      "id": 5891,
      "verseKey": "84:7",
      "verseNumber": 7,
      "textUthmani": "فَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِيَمِينِهِۦ",
      "textSimple": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ"
    },
    {
      "id": 5892,
      "verseKey": "84:8",
      "verseNumber": 8,
      "textUthmani": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا",
      "textSimple": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا"
    },
    {
      "id": 5893,
      "verseKey": "84:9",
      "verseNumber": 9,
      "textUthmani": "وَيَنقَلِبُ إِلَىٰٓ أَهْلِهِۦ مَسْرُورًا",
      "textSimple": "وَيَنقَلِبُ إِلَىٰ أَهْلِهِ مَسْرُورًا"
    },
    {
      "id": 5894,
      "verseKey": "84:10",
      "verseNumber": 10,
      "textUthmani": "وَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ وَرَآءَ ظَهْرِهِۦ",
      "textSimple": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ وَرَاءَ ظَهْرِهِ"
    },
    {
      "id": 5895,
      "verseKey": "84:11",
      "verseNumber": 11,
      "textUthmani": "فَسَوْفَ يَدْعُوا۟ ثُبُورًا",
      "textSimple": "فَسَوْفَ يَدْعُو ثُبُورًا"
    },
    {
      "id": 5896,
      "verseKey": "84:12",
      "verseNumber": 12,
      "textUthmani": "وَيَصْلَىٰ سَعِيرًا",
      "textSimple": "وَيَصْلَىٰ سَعِيرًا"
    },
    {
      "id": 5897,
      "verseKey": "84:13",
      "verseNumber": 13,
      "textUthmani": "إِنَّهُۥ كَانَ فِىٓ أَهْلِهِۦ مَسْرُورًا",
      "textSimple": "إِنَّهُ كَانَ فِي أَهْلِهِ مَسْرُورًا"
    },
    {
      "id": 5898,
      "verseKey": "84:14",
      "verseNumber": 14,
      "textUthmani": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ",
      "textSimple": "إِنَّهُ ظَنَّ أَن لَّن يَحُورَ"
    },
    {
      "id": 5899,
      "verseKey": "84:15",
      "verseNumber": 15,
      "textUthmani": "بَلَىٰٓ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرًا",
      "textSimple": "بَلَىٰ إِنَّ رَبَّهُ كَانَ بِهِ بَصِيرًا"
    },
    {
      "id": 5900,
      "verseKey": "84:16",
      "verseNumber": 16,
      "textUthmani": "فَلَآ أُقْسِمُ بِٱلشَّفَقِ",
      "textSimple": "فَلَا أُقْسِمُ بِالشَّفَقِ"
    },
    {
      "id": 5901,
      "verseKey": "84:17",
      "verseNumber": 17,
      "textUthmani": "وَٱلَّيْلِ وَمَا وَسَقَ",
      "textSimple": "وَاللَّيْلِ وَمَا وَسَقَ"
    },
    {
      "id": 5902,
      "verseKey": "84:18",
      "verseNumber": 18,
      "textUthmani": "وَٱلْقَمَرِ إِذَا ٱتَّسَقَ",
      "textSimple": "وَالْقَمَرِ إِذَا اتَّسَقَ"
    },
    {
      "id": 5903,
      "verseKey": "84:19",
      "verseNumber": 19,
      "textUthmani": "لَتَرْكَبُنَّ طَبَقًا عَن طَبَقٍ",
      "textSimple": "لَتَرْكَبُنَّ طَبَقًا عَن طَبَقٍ"
    },
    {
      "id": 5904,
      "verseKey": "84:20",
      "verseNumber": 20,
      "textUthmani": "فَمَا لَهُمْ لَا يُؤْمِنُونَ",
      "textSimple": "فَمَا لَهُمْ لَا يُؤْمِنُونَ"
    },
    {
      "id": 5905,
      "verseKey": "84:21",
      "verseNumber": 21,
      "textUthmani": "وَإِذَا قُرِئَ عَلَيْهِمُ ٱلْقُرْءَانُ لَا يَسْجُدُونَ ۩",
      "textSimple": "وَإِذَا قُرِئَ عَلَيْهِمُ الْقُرْآنُ لَا يَسْجُدُونَ"
    },
    {
      "id": 5906,
      "verseKey": "84:22",
      "verseNumber": 22,
      "textUthmani": "بَلِ ٱلَّذِينَ كَفَرُوا۟ يُكَذِّبُونَ",
      "textSimple": "بَلِ الَّذِينَ كَفَرُوا يُكَذِّبُونَ"
    },
    {
      "id": 5907,
      "verseKey": "84:23",
      "verseNumber": 23,
      "textUthmani": "وَٱللَّهُ أَعْلَمُ بِمَا يُوعُونَ",
      "textSimple": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَ"
    },
    {
      "id": 5908,
      "verseKey": "84:24",
      "verseNumber": 24,
      "textUthmani": "فَبَشِّرْهُم بِعَذَابٍ أَلِيمٍ",
      "textSimple": "فَبَشِّرْهُم بِعَذَابٍ أَلِيمٍ"
    },
    {
      "id": 5909,
      "verseKey": "84:25",
      "verseNumber": 25,
      "textUthmani": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۭ",
      "textSimple": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"
    }
  ]
}