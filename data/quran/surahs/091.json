{
  "id": 91,
  "nameArabic": "الشمس",
  "nameSimple": "Ash-Shams",
  "nameComplex": "Ash-Shams",
  "nameTranslation": "The Sun",
  "revelationType": "makkah",
  "versesCount": 15,
  "verses": [
    {
      "id": 6044,
      "verseKey": "91:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلشَّمْسِ وَضُحَىٰهَا",
      "textSimple": "وَالشَّمْسِ وَضُحَاهَا"
    },
    {
      "id": 6045,
      "verseKey": "91:2",
      "verseNumber": 2,
      "textUthmani": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
      "textSimple": "وَالْقَمَرِ إِذَا تَلَاهَا"
    },
    {
      "id": 6046,
      "verseKey": "91:3",
      "verseNumber": 3,
      "textUthmani": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "textSimple": "وَالنَّهَارِ إِذَا جَلَّاهَا"
    },
    {
      "id": 6047,
      "verseKey": "91:4",
      "verseNumber": 4,
      "textUthmani": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
      "textSimple": "وَاللَّيْلِ إِذَا يَغْشَاهَا"
    },
    {
      "id": 6048,
      "verseKey": "91:5",
      "verseNumber": 5,
      "textUthmani": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "textSimple": "وَالسَّمَاءِ وَمَا بَنَاهَا"
    },
    {
      "id": 6049,
      "verseKey": "91:6",
      "verseNumber": 6,
      "textUthmani": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
      "textSimple": "وَالْأَرْضِ وَمَا طَحَاهَا"
    },
    {
      "id": 6050,
      "verseKey": "91:7",
      "verseNumber": 7,
      "textUthmani": "وَنَفْسٍ وَمَا سَوَّىٰهَا",
      "textSimple": "وَنَفْسٍ وَمَا سَوَّاهَا"
    },
    {
      "id": 6051,
      "verseKey": "91:8",
      "verseNumber": 8,
      "textUthmani": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
      "textSimple": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا"
    },
    {
      "id": 6052,
      "verseKey": "91:9",
      "verseNumber": 9,
      "textUthmani": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
      "textSimple": "قَدْ أَفْلَحَ مَن زَكَّاهَا"
    },
    {
      "id": 6053,
      "verseKey": "91:10",
      "verseNumber": 10,
      "textUthmani": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
      "textSimple": "وَقَدْ خَابَ مَن دَسَّاهَا"
    },
    {
      "id": 6054,
      "verseKey": "91:11",
      "verseNumber": 11,
      "textUthmani": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
      "textSimple": "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا"
    },
    {
      "id": 6055,
      "verseKey": "91:12",
      "verseNumber": 12,
      "textUthmani": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
      "textSimple": "إِذِ انبَعَثَ أَشْقَاهَا"
    },
    {
      "id": 6056,
      "verseKey": "91:13",
      "verseNumber": 13,
      "textUthmani": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَـٰهَا",
      "textSimple": "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا"
    },
    {
      "id": 6057,
      "verseKey": "91:14",
      "verseNumber": 14,
      "textUthmani": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
      "textSimple": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنبِهِمْ فَسَوَّاهَا"
    },
    {
      "id": 6058,
      "verseKey": "91:15",
      "verseNumber": 15,
      "textUthmani": "وَلَا يَخَافُ عُقْبَـٰهَا",
      "textSimple": "وَلَا يَخَافُ عُقْبَاهَا"
    }
  ]
}