{
  "id": 89,
  "nameArabic": "الفجر",
  "nameSimple": "Al-Fajr",
  "nameComplex": "Al-Fajr",
  "nameTranslation": "The Dawn",
  "revelationType": "makkah",
  "versesCount": 30,
  "verses": [
    {
      "id": 5994,
      "verseKey": "89:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلْفَجْرِ",
      "textSimple": "وَالْفَجْرِ"
    },
    {
      "id": 5995,
      "verseKey": "89:2",
      "verseNumber": 2,
      "textUthmani": "وَلَيَالٍ عَشْرٍ",
      "textSimple": "وَلَيَالٍ عَشْرٍ"
    },
    {
      "id": 5996,
      "verseKey": "89:3",
      "verseNumber": 3,
      "textUthmani": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
      "textSimple": "وَالشَّفْعِ وَالْوَتْرِ"
    },
    {
      "id": 5997,
      "verseKey": "89:4",
      "verseNumber": 4,
      "textUthmani": "وَٱلَّيْلِ إِذَا يَسْرِ",
      "textSimple": "وَاللَّيْلِ إِذَا يَسْرِ"
    },
    {
      "id": 5998,
      "verseKey": "89:5",
      "verseNumber": 5,
      "textUthmani": "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ",
      "textSimple": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِّذِي حِجْرٍ"
    },
    {
      "id": 5999,
      "verseKey": "89:6",
      "verseNumber": 6,
      "textUthmani": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "textSimple": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
    },
    {
      "id": 6000,
      "verseKey": "89:7",
      "verseNumber": 7,
      "textUthmani": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
      "textSimple": "إِرَمَ ذَاتِ الْعِمَادِ"
    },
    {
      "id": 6001,
      "verseKey": "89:8",
      "verseNumber": 8,
      "textUthmani": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَـٰدِ",
      "textSimple": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"
    },
    {
      "id": 6002,
      "verseKey": "89:9",
      "verseNumber": 9,
      "textUthmani": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
      "textSimple": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ"
    },
    {
      "id": 6003,
      "verseKey": "89:10",
      "verseNumber": 10,
      "textUthmani": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
      "textSimple": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ"
    },
    {
      "id": 6004,
      "verseKey": "89:11",
      "verseNumber": 11,
      "textUthmani": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَـٰدِ",
      "textSimple": "الَّذِينَ طَغَوْا فِي الْبِلَادِ"
    },
    {
      "id": 6005,
      "verseKey": "89:12",
      "verseNumber": 12,
      "textUthmani": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
      "textSimple": "فَأَكْثَرُوا فِيهَا الْفَسَادَ"
    },
    {
      "id": 6006,
      "verseKey": "89:13",
      "verseNumber": 13,
      "textUthmani": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
      "textSimple": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
    },
    {
      "id": 6007,
      "verseKey": "89:14",
      "verseNumber": 14,
      "textUthmani": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
      "textSimple": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ"
    },
    {
      "id": 6008,
      "verseKey": "89:15",
      "verseNumber": 15,
      "textUthmani": "فَأَمَّا ٱلْإِنسَـٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
      "textSimple": "فَأَمَّا الْإِنسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ"
    },
    {
      "id": 6009,
      "verseKey": "89:16",
      "verseNumber": 16,
      "textUthmani": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَـٰنَنِ",
      "textSimple": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ"
    },
    {
      "id": 6010,
      "verseKey": "89:17",
      "verseNumber": 17,
      "textUthmani": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
      "textSimple": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ الْيَتِيمَ"
    },
    {
      "id": 6011,
      "verseKey": "89:18",
      "verseNumber": 18,
      "textUthmani": "وَلَا تَحَـٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "textSimple": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ"
    },
    {
      "id": 6012,
      "verseKey": "89:19",
      "verseNumber": 19,
      "textUthmani": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا",
      "textSimple": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَّمًّا"
    },
    {
      "id": 6013,
      "verseKey": "89:20",
      "verseNumber": 20,
      "textUthmani": "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا",
      "textSimple": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا"
    },
    {
      "id": 6014,
      "verseKey": "89:21",
      "verseNumber": 21,
      "textUthmani": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا",
      "textSimple": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا"
    },
    {
      "id": 6015,
      "verseKey": "89:22",
      "verseNumber": 22,
      "textUthmani": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا",
      "textSimple": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"
    },
    {
      "id": 6016,
      "verseKey": "89:23",
      "verseNumber": 23,
      "textUthmani": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَـٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
      "textSimple": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ"
    },
    {
      "id": 6017,
      "verseKey": "89:24",
      "verseNumber": 24,
      "textUthmani": "يَقُولُ يَـٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
      "textSimple": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي"
    },
    {
      "id": 6018,
      "verseKey": "89:25",
      "verseNumber": 25,
      "textUthmani": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ",
      "textSimple": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُ أَحَدٌ"
    },
    {
      "id": 6019,
      "verseKey": "89:26",
      "verseNumber": 26,
      "textUthmani": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ",
      "textSimple": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ"
    },
    {
      "id": 6020,
      "verseKey": "89:27",
      "verseNumber": 27,
      "textUthmani": "يَـٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
      "textSimple": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"
    },
    {
      "id": 6021,
      "verseKey": "89:28",
      "verseNumber": 28,
      "textUthmani": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً",
      "textSimple": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً"
    },
    {
      "id": 6022,
      "verseKey": "89:29",
      "verseNumber": 29,
      "textUthmani": "فَٱدْخُلِى فِى عِبَـٰدِى",
      "textSimple": "فَادْخُلِي فِي عِبَادِي"
    },
    {
      "id": 6023,
      "verseKey": "89:30",
      "verseNumber": 30,
      "textUthmani": "وَٱدْخُلِى جَنَّتِى",
      "textSimple": "وَادْخُلِي جَنَّتِي"
    }
  ]
}