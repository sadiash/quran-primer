{
  "id": 78,
  "nameArabic": "النبإ",
  "nameSimple": "An-Naba",
  "nameComplex": "An-Naba",
  "nameTranslation": "The Tidings",
  "revelationType": "makkah",
  "versesCount": 40,
  "verses": [
    {
      "id": 5673,
      "verseKey": "78:1",
      "verseNumber": 1,
      "textUthmani": " عَمَّ يَتَسَآءَلُونَ",
      "textSimple": "عَمَّ يَتَسَاءَلُونَ"
    },
    {
      "id": 5674,
      "verseKey": "78:2",
      "verseNumber": 2,
      "textUthmani": "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ",
      "textSimple": "عَنِ النَّبَإِ الْعَظِيمِ"
    },
    {
      "id": 5675,
      "verseKey": "78:3",
      "verseNumber": 3,
      "textUthmani": "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ",
      "textSimple": "الَّذِي هُمْ فِيهِ مُخْتَلِفُونَ"
    },
    {
      "id": 5676,
      "verseKey": "78:4",
      "verseNumber": 4,
      "textUthmani": "كَلَّا سَيَعْلَمُونَ",
      "textSimple": "كَلَّا سَيَعْلَمُونَ"
    },
    {
      "id": 5677,
      "verseKey": "78:5",
      "verseNumber": 5,
      "textUthmani": "ثُمَّ كَلَّا سَيَعْلَمُونَ",
      "textSimple": "ثُمَّ كَلَّا سَيَعْلَمُونَ"
    },
    {
      "id": 5678,
      "verseKey": "78:6",
      "verseNumber": 6,
      "textUthmani": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَـٰدًا",
      "textSimple": "أَلَمْ نَجْعَلِ الْأَرْضَ مِهَادًا"
    },
    {
      "id": 5679,
      "verseKey": "78:7",
      "verseNumber": 7,
      "textUthmani": "وَٱلْجِبَالَ أَوْتَادًا",
      "textSimple": "وَالْجِبَالَ أَوْتَادًا"
    },
    {
      "id": 5680,
      "verseKey": "78:8",
      "verseNumber": 8,
      "textUthmani": "وَخَلَقْنَـٰكُمْ أَزْوَٰجًا",
      "textSimple": "وَخَلَقْنَاكُمْ أَزْوَاجًا"
    },
    {
      "id": 5681,
      "verseKey": "78:9",
      "verseNumber": 9,
      "textUthmani": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا",
      "textSimple": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا"
    },
    {
      "id": 5682,
      "verseKey": "78:10",
      "verseNumber": 10,
      "textUthmani": "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًا",
      "textSimple": "وَجَعَلْنَا اللَّيْلَ لِبَاسًا"
    },
    {
      "id": 5683,
      "verseKey": "78:11",
      "verseNumber": 11,
      "textUthmani": "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًا",
      "textSimple": "وَجَعَلْنَا النَّهَارَ مَعَاشًا"
    },
    {
      "id": 5684,
      "verseKey": "78:12",
      "verseNumber": 12,
      "textUthmani": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا",
      "textSimple": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا"
    },
    {
      "id": 5685,
      "verseKey": "78:13",
      "verseNumber": 13,
      "textUthmani": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا",
      "textSimple": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا"
    },
    {
      "id": 5686,
      "verseKey": "78:14",
      "verseNumber": 14,
      "textUthmani": "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءً ثَجَّاجًا",
      "textSimple": "وَأَنزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا"
    },
    {
      "id": 5687,
      "verseKey": "78:15",
      "verseNumber": 15,
      "textUthmani": "لِّنُخْرِجَ بِهِۦ حَبًّا وَنَبَاتًا",
      "textSimple": "لِّنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا"
    },
    {
      "id": 5688,
      "verseKey": "78:16",
      "verseNumber": 16,
      "textUthmani": "وَجَنَّـٰتٍ أَلْفَافًا",
      "textSimple": "وَجَنَّاتٍ أَلْفَافًا"
    },
    {
      "id": 5689,
      "verseKey": "78:17",
      "verseNumber": 17,
      "textUthmani": "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَـٰتًا",
      "textSimple": "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا"
    },
    {
      "id": 5690,
      "verseKey": "78:18",
      "verseNumber": 18,
      "textUthmani": "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًا",
      "textSimple": "يَوْمَ يُنفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا"
    },
    {
      "id": 5691,
      "verseKey": "78:19",
      "verseNumber": 19,
      "textUthmani": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًا",
      "textSimple": "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا"
    },
    {
      "id": 5692,
      "verseKey": "78:20",
      "verseNumber": 20,
      "textUthmani": "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا",
      "textSimple": "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا"
    },
    {
      "id": 5693,
      "verseKey": "78:21",
      "verseNumber": 21,
      "textUthmani": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا",
      "textSimple": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا"
    },
    {
      "id": 5694,
      "verseKey": "78:22",
      "verseNumber": 22,
      "textUthmani": "لِّلطَّـٰغِينَ مَـَٔابًا",
      "textSimple": "لِّلطَّاغِينَ مَآبًا"
    },
    {
      "id": 5695,
      "verseKey": "78:23",
      "verseNumber": 23,
      "textUthmani": "لَّـٰبِثِينَ فِيهَآ أَحْقَابًا",
      "textSimple": "لَّابِثِينَ فِيهَا أَحْقَابًا"
    },
    {
      "id": 5696,
      "verseKey": "78:24",
      "verseNumber": 24,
      "textUthmani": "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا",
      "textSimple": "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا"
    },
    {
      "id": 5697,
      "verseKey": "78:25",
      "verseNumber": 25,
      "textUthmani": "إِلَّا حَمِيمًا وَغَسَّاقًا",
      "textSimple": "إِلَّا حَمِيمًا وَغَسَّاقًا"
    },
    {
      "id": 5698,
      "verseKey": "78:26",
      "verseNumber": 26,
      "textUthmani": "جَزَآءً وِفَاقًا",
      "textSimple": "جَزَاءً وِفَاقًا"
    },
    {
      "id": 5699,
      "verseKey": "78:27",
      "verseNumber": 27,
      "textUthmani": "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًا",
      "textSimple": "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا"
    },
    {
      "id": 5700,
      "verseKey": "78:28",
      "verseNumber": 28,
      "textUthmani": "وَكَذَّبُوا۟ بِـَٔايَـٰتِنَا كِذَّابًا",
      "textSimple": "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا"
    },
    {
      "id": 5701,
      "verseKey": "78:29",
      "verseNumber": 29,
      "textUthmani": "وَكُلَّ شَىْءٍ أَحْصَيْنَـٰهُ كِتَـٰبًا",
      "textSimple": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا"
    },
    {
      "id": 5702,
      "verseKey": "78:30",
      "verseNumber": 30,
      "textUthmani": "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا",
      "textSimple": "فَذُوقُوا فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا"
    },
    {
      "id": 5703,
      "verseKey": "78:31",
      "verseNumber": 31,
      "textUthmani": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
      "textSimple": "إِنَّ لِلْمُتَّقِينَ مَفَازًا"
    },
    {
      "id": 5704,
      "verseKey": "78:32",
      "verseNumber": 32,
      "textUthmani": "حَدَآئِقَ وَأَعْنَـٰبًا",
      "textSimple": "حَدَائِقَ وَأَعْنَابًا"
    },
    {
      "id": 5705,
      "verseKey": "78:33",
      "verseNumber": 33,
      "textUthmani": "وَكَوَاعِبَ أَتْرَابًا",
      "textSimple": "وَكَوَاعِبَ أَتْرَابًا"
    },
    {
      "id": 5706,
      "verseKey": "78:34",
      "verseNumber": 34,
      "textUthmani": "وَكَأْسًا دِهَاقًا",
      "textSimple": "وَكَأْسًا دِهَاقًا"
    },
    {
      "id": 5707,
      "verseKey": "78:35",
      "verseNumber": 35,
      "textUthmani": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّٰبًا",
      "textSimple": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا"
    },
    {
      "id": 5708,
      "verseKey": "78:36",
      "verseNumber": 36,
      "textUthmani": "جَزَآءً مِّن رَّبِّكَ عَطَآءً حِسَابًا",
      "textSimple": "جَزَاءً مِّن رَّبِّكَ عَطَاءً حِسَابًا"
    },
    {
      "id": 5709,
      "verseKey": "78:37",
      "verseNumber": 37,
      "textUthmani": "رَّبِّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَـٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا",
      "textSimple": "رَّبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا"
    },
    {
      "id": 5710,
      "verseKey": "78:38",
      "verseNumber": 38,
      "textUthmani": "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَـٰٓئِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَـٰنُ وَقَالَ صَوَابًا",
      "textSimple": "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَٰنُ وَقَالَ صَوَابًا"
    },
    {
      "id": 5711,
      "verseKey": "78:39",
      "verseNumber": 39,
      "textUthmani": "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
      "textSimple": "ذَٰلِكَ الْيَوْمُ الْحَقُّ ۖ فَمَن شَاءَ اتَّخَذَ إِلَىٰ رَبِّهِ مَآبًا"
    },
    {
      "id": 5712,
      "verseKey": "78:40",
      "verseNumber": 40,
      "textUthmani": "إِنَّآ أَنذَرْنَـٰكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَـٰلَيْتَنِى كُنتُ تُرَٰبًۢا",
      "textSimple": "إِنَّا أَنذَرْنَاكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ الْكَافِرُ يَا لَيْتَنِي كُنتُ تُرَابًا"
    }
  ]
}