{
  "id": 85,
  "nameArabic": "البروج",
  "nameSimple": "Al-Buruj",
  "nameComplex": "Al-Burūj",
  "nameTranslation": "The Mansions of the Stars",
  "revelationType": "makkah",
  "versesCount": 22,
  "verses": [
    {
      "id": 5910,
      "verseKey": "85:1",
      "verseNumber": 1,
      "textUthmani": " وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
      "textSimple": "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ"
    },
    {
      "id": 5911,
      "verseKey": "85:2",
      "verseNumber": 2,
      "textUthmani": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
      "textSimple": "وَالْيَوْمِ الْمَوْعُودِ"
    },
    {
      "id": 5912,
      "verseKey": "85:3",
      "verseNumber": 3,
      "textUthmani": "وَشَاهِدٍ وَمَشْهُودٍ",
      "textSimple": "وَشَاهِدٍ وَمَشْهُودٍ"
    },
    {
      "id": 5913,
      "verseKey": "85:4",
      "verseNumber": 4,
      "textUthmani": "قُتِلَ أَصْحَـٰبُ ٱلْأُخْدُودِ",
      "textSimple": "قُتِلَ أَصْحَابُ الْأُخْدُودِ"
    },
    {
      "id": 5914,
      "verseKey": "85:5",
      "verseNumber": 5,
      "textUthmani": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
      "textSimple": "النَّارِ ذَاتِ الْوَقُودِ"
    },
    {
      "id": 5915,
      "verseKey": "85:6",
      "verseNumber": 6,
      "textUthmani": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
      "textSimple": "إِذْ هُمْ عَلَيْهَا قُعُودٌ"
    },
    {
      "id": 5916,
      "verseKey": "85:7",
      "verseNumber": 7,
      "textUthmani": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ",
      "textSimple": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ"
    },
    {
      "id": 5917,
      "verseKey": "85:8",
      "verseNumber": 8,
      "textUthmani": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
      "textSimple": "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَن يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ"
    },
    {
      "id": 5918,
      "verseKey": "85:9",
      "verseNumber": 9,
      "textUthmani": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ",
      "textSimple": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ"
    },
    {
      "id": 5919,
      "verseKey": "85:10",
      "verseNumber": 10,
      "textUthmani": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَـٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
      "textSimple": "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ"
    },
    {
      "id": 5920,
      "verseKey": "85:11",
      "verseNumber": 11,
      "textUthmani": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ جَنَّـٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
      "textSimple": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِن تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ"
    },
    {
      "id": 5921,
      "verseKey": "85:12",
      "verseNumber": 12,
      "textUthmani": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
      "textSimple": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ"
    },
    {
      "id": 5922,
      "verseKey": "85:13",
      "verseNumber": 13,
      "textUthmani": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
      "textSimple": "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ"
    },
    {
      "id": 5923,
      "verseKey": "85:14",
      "verseNumber": 14,
      "textUthmani": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
      "textSimple": "وَهُوَ الْغَفُورُ الْوَدُودُ"
    },
    {
      "id": 5924,
      "verseKey": "85:15",
      "verseNumber": 15,
      "textUthmani": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
      "textSimple": "ذُو الْعَرْشِ الْمَجِيدُ"
    },
    {
      "id": 5925,
      "verseKey": "85:16",
      "verseNumber": 16,
      "textUthmani": "فَعَّالٌ لِّمَا يُرِيدُ",
      "textSimple": "فَعَّالٌ لِّمَا يُرِيدُ"
    },
    {
      "id": 5926,
      "verseKey": "85:17",
      "verseNumber": 17,
      "textUthmani": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
      "textSimple": "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ"
    },
    {
      "id": 5927,
      "verseKey": "85:18",
      "verseNumber": 18,
      "textUthmani": "فِرْعَوْنَ وَثَمُودَ",
      "textSimple": "فِرْعَوْنَ وَثَمُودَ"
    },
    {
      "id": 5928,
      "verseKey": "85:19",
      "verseNumber": 19,
      "textUthmani": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ",
      "textSimple": "بَلِ الَّذِينَ كَفَرُوا فِي تَكْذِيبٍ"
    },
    {
      "id": 5929,
      "verseKey": "85:20",
      "verseNumber": 20,
      "textUthmani": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ",
      "textSimple": "وَاللَّهُ مِن وَرَائِهِم مُّحِيطٌ"
    },
    {
      "id": 5930,
      "verseKey": "85:21",
      "verseNumber": 21,
      "textUthmani": "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ",
      "textSimple": "بَلْ هُوَ قُرْآنٌ مَّجِيدٌ"
    },
    {
      "id": 5931,
      "verseKey": "85:22",
      "verseNumber": 22,
      "textUthmani": "فِى لَوْحٍ مَّحْفُوظٍۭ",
      "textSimple": "فِي لَوْحٍ مَّحْفُوظٍ"
    }
  ]
}