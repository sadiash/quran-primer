{
  "id": 69,
  "nameArabic": "الحاقة",
  "nameSimple": "Al-Haqqah",
  "nameComplex": "Al-Ĥāqqah",
  "nameTranslation": "The Reality",
  "revelationType": "makkah",
  "versesCount": 52,
  "verses": [
    {
      "id": 5324,
      "verseKey": "69:1",
      "verseNumber": 1,
      "textUthmani": " ٱلْحَآقَّةُ",
      "textSimple": "الْحَاقَّةُ"
    },
    {
      "id": 5325,
      "verseKey": "69:2",
      "verseNumber": 2,
      "textUthmani": "مَا ٱلْحَآقَّةُ",
      "textSimple": "مَا الْحَاقَّةُ"
    },
    {
      "id": 5326,
      "verseKey": "69:3",
      "verseNumber": 3,
      "textUthmani": "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ",
      "textSimple": "وَمَا أَدْرَاكَ مَا الْحَاقَّةُ"
    },
    {
      "id": 5327,
      "verseKey": "69:4",
      "verseNumber": 4,
      "textUthmani": "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ",
      "textSimple": "كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ"
    },
    {
      "id": 5328,
      "verseKey": "69:5",
      "verseNumber": 5,
      "textUthmani": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ",
      "textSimple": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ"
    },
    {
      "id": 5329,
      "verseKey": "69:6",
      "verseNumber": 6,
      "textUthmani": "وَأَمَّا عَادٌ فَأُهْلِكُوا۟ بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ",
      "textSimple": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
    },
    {
      "id": 5330,
      "verseKey": "69:7",
      "verseNumber": 7,
      "textUthmani": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَـٰنِيَةَ أَيَّامٍ حُسُومًا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ",
      "textSimple": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا فَتَرَى الْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
    },
    {
      "id": 5331,
      "verseKey": "69:8",
      "verseNumber": 8,
      "textUthmani": "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍ",
      "textSimple": "فَهَلْ تَرَىٰ لَهُم مِّن بَاقِيَةٍ"
    },
    {
      "id": 5332,
      "verseKey": "69:9",
      "verseNumber": 9,
      "textUthmani": "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَـٰتُ بِٱلْخَاطِئَةِ",
      "textSimple": "وَجَاءَ فِرْعَوْنُ وَمَن قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ"
    },
    {
      "id": 5333,
      "verseKey": "69:10",
      "verseNumber": 10,
      "textUthmani": "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً",
      "textSimple": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً"
    },
    {
      "id": 5334,
      "verseKey": "69:11",
      "verseNumber": 11,
      "textUthmani": "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَـٰكُمْ فِى ٱلْجَارِيَةِ",
      "textSimple": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ"
    },
    {
      "id": 5335,
      "verseKey": "69:12",
      "verseNumber": 12,
      "textUthmani": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَآ أُذُنٌ وَٰعِيَةٌ",
      "textSimple": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ"
    },
    {
      "id": 5336,
      "verseKey": "69:13",
      "verseNumber": 13,
      "textUthmani": "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌ وَٰحِدَةٌ",
      "textSimple": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ"
    },
    {
      "id": 5337,
      "verseKey": "69:14",
      "verseNumber": 14,
      "textUthmani": "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةً وَٰحِدَةً",
      "textSimple": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً"
    },
    {
      "id": 5338,
      "verseKey": "69:15",
      "verseNumber": 15,
      "textUthmani": "فَيَوْمَئِذٍ وَقَعَتِ ٱلْوَاقِعَةُ",
      "textSimple": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ"
    },
    {
      "id": 5339,
      "verseKey": "69:16",
      "verseNumber": 16,
      "textUthmani": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍ وَاهِيَةٌ",
      "textSimple": "وَانشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ"
    },
    {
      "id": 5340,
      "verseKey": "69:17",
      "verseNumber": 17,
      "textUthmani": "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَـٰنِيَةٌ",
      "textSimple": "وَالْمَلَكُ عَلَىٰ أَرْجَائِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ"
    },
    {
      "id": 5341,
      "verseKey": "69:18",
      "verseNumber": 18,
      "textUthmani": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ",
      "textSimple": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ"
    },
    {
      "id": 5342,
      "verseKey": "69:19",
      "verseNumber": 19,
      "textUthmani": "فَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَـٰبِيَهْ",
      "textSimple": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ"
    },
    {
      "id": 5343,
      "verseKey": "69:20",
      "verseNumber": 20,
      "textUthmani": "إِنِّى ظَنَنتُ أَنِّى مُلَـٰقٍ حِسَابِيَهْ",
      "textSimple": "إِنِّي ظَنَنتُ أَنِّي مُلَاقٍ حِسَابِيَهْ"
    },
    {
      "id": 5344,
      "verseKey": "69:21",
      "verseNumber": 21,
      "textUthmani": "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ",
      "textSimple": "فَهُوَ فِي عِيشَةٍ رَّاضِيَةٍ"
    },
    {
      "id": 5345,
      "verseKey": "69:22",
      "verseNumber": 22,
      "textUthmani": "فِى جَنَّةٍ عَالِيَةٍ",
      "textSimple": "فِي جَنَّةٍ عَالِيَةٍ"
    },
    {
      "id": 5346,
      "verseKey": "69:23",
      "verseNumber": 23,
      "textUthmani": "قُطُوفُهَا دَانِيَةٌ",
      "textSimple": "قُطُوفُهَا دَانِيَةٌ"
    },
    {
      "id": 5347,
      "verseKey": "69:24",
      "verseNumber": 24,
      "textUthmani": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ",
      "textSimple": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا أَسْلَفْتُمْ فِي الْأَيَّامِ الْخَالِيَةِ"
    },
    {
      "id": 5348,
      "verseKey": "69:25",
      "verseNumber": 25,
      "textUthmani": "وَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَـٰلَيْتَنِى لَمْ أُوتَ كِتَـٰبِيَهْ",
      "textSimple": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِشِمَالِهِ فَيَقُولُ يَا لَيْتَنِي لَمْ أُوتَ كِتَابِيَهْ"
    },
    {
      "id": 5349,
      "verseKey": "69:26",
      "verseNumber": 26,
      "textUthmani": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ",
      "textSimple": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
    },
    {
      "id": 5350,
      "verseKey": "69:27",
      "verseNumber": 27,
      "textUthmani": "يَـٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ",
      "textSimple": "يَا لَيْتَهَا كَانَتِ الْقَاضِيَةَ"
    },
    {
      "id": 5351,
      "verseKey": "69:28",
      "verseNumber": 28,
      "textUthmani": "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ",
      "textSimple": "مَا أَغْنَىٰ عَنِّي مَالِيَهْ ۜ"
    },
    {
      "id": 5352,
      "verseKey": "69:29",
      "verseNumber": 29,
      "textUthmani": "هَلَكَ عَنِّى سُلْطَـٰنِيَهْ",
      "textSimple": "هَلَكَ عَنِّي سُلْطَانِيَهْ"
    },
    {
      "id": 5353,
      "verseKey": "69:30",
      "verseNumber": 30,
      "textUthmani": "خُذُوهُ فَغُلُّوهُ",
      "textSimple": "خُذُوهُ فَغُلُّوهُ"
    },
    {
      "id": 5354,
      "verseKey": "69:31",
      "verseNumber": 31,
      "textUthmani": "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ",
      "textSimple": "ثُمَّ الْجَحِيمَ صَلُّوهُ"
    },
    {
      "id": 5355,
      "verseKey": "69:32",
      "verseNumber": 32,
      "textUthmani": "ثُمَّ فِى سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَٱسْلُكُوهُ",
      "textSimple": "ثُمَّ فِي سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَاسْلُكُوهُ"
    },
    {
      "id": 5356,
      "verseKey": "69:33",
      "verseNumber": 33,
      "textUthmani": "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ",
      "textSimple": "إِنَّهُ كَانَ لَا يُؤْمِنُ بِاللَّهِ الْعَظِيمِ"
    },
    {
      "id": 5357,
      "verseKey": "69:34",
      "verseNumber": 34,
      "textUthmani": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "textSimple": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ"
    },
    {
      "id": 5358,
      "verseKey": "69:35",
      "verseNumber": 35,
      "textUthmani": "فَلَيْسَ لَهُ ٱلْيَوْمَ هَـٰهُنَا حَمِيمٌ",
      "textSimple": "فَلَيْسَ لَهُ الْيَوْمَ هَاهُنَا حَمِيمٌ"
    },
    {
      "id": 5359,
      "verseKey": "69:36",
      "verseNumber": 36,
      "textUthmani": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ",
      "textSimple": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
    },
    {
      "id": 5360,
      "verseKey": "69:37",
      "verseNumber": 37,
      "textUthmani": "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَـٰطِـُٔونَ",
      "textSimple": "لَّا يَأْكُلُهُ إِلَّا الْخَاطِئُونَ"
    },
    {
      "id": 5361,
      "verseKey": "69:38",
      "verseNumber": 38,
      "textUthmani": "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ",
      "textSimple": "فَلَا أُقْسِمُ بِمَا تُبْصِرُونَ"
    },
    {
      "id": 5362,
      "verseKey": "69:39",
      "verseNumber": 39,
      "textUthmani": "وَمَا لَا تُبْصِرُونَ",
      "textSimple": "وَمَا لَا تُبْصِرُونَ"
    },
    {
      "id": 5363,
      "verseKey": "69:40",
      "verseNumber": 40,
      "textUthmani": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "textSimple": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
    },
    {
      "id": 5364,
      "verseKey": "69:41",
      "verseNumber": 41,
      "textUthmani": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ",
      "textSimple": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ"
    },
    {
      "id": 5365,
      "verseKey": "69:42",
      "verseNumber": 42,
      "textUthmani": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ",
      "textSimple": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ"
    },
    {
      "id": 5366,
      "verseKey": "69:43",
      "verseNumber": 43,
      "textUthmani": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ",
      "textSimple": "تَنزِيلٌ مِّن رَّبِّ الْعَالَمِينَ"
    },
    {
      "id": 5367,
      "verseKey": "69:44",
      "verseNumber": 44,
      "textUthmani": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ",
      "textSimple": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ الْأَقَاوِيلِ"
    },
    {
      "id": 5368,
      "verseKey": "69:45",
      "verseNumber": 45,
      "textUthmani": "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ",
      "textSimple": "لَأَخَذْنَا مِنْهُ بِالْيَمِينِ"
    },
    {
      "id": 5369,
      "verseKey": "69:46",
      "verseNumber": 46,
      "textUthmani": "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ",
      "textSimple": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِينَ"
    },
    {
      "id": 5370,
      "verseKey": "69:47",
      "verseNumber": 47,
      "textUthmani": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَـٰجِزِينَ",
      "textSimple": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَاجِزِينَ"
    },
    {
      "id": 5371,
      "verseKey": "69:48",
      "verseNumber": 48,
      "textUthmani": "وَإِنَّهُۥ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ",
      "textSimple": "وَإِنَّهُ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ"
    },
    {
      "id": 5372,
      "verseKey": "69:49",
      "verseNumber": 49,
      "textUthmani": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
      "textSimple": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ"
    },
    {
      "id": 5373,
